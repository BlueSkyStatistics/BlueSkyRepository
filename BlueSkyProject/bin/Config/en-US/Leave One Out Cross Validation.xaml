<BSkyCanvas internalhelpfilename="{x:Null}" Title="Tuning a Model with Leave One Out Cross Validation" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="caret" CanExecute="True" PrereqCommandString="{x:Null}" CommandString="{}{{modelname}}=character(0)&#xA;local (&#xA;{&#xA; #get short name for  selected model &#xA; #modelshortname = getModelInitials(&quot;{{modelselected}}&quot;)&#xA; #load package for selected model like nb , rf &#xA; modelselected = &quot;{{ModelType}}&quot; &#xA; success = loadReqModelPackage(modelselected )&#xA; if(success)&#xA; {&#xA;  if (&quot;tbl&quot; %in% class({{%DATASET%}}))&#xA;  {&#xA;  TrainData &lt;- {{%DATASET%}}[,{{independent}}]&#xA;  TrainClasses  &lt;- {{%DATASET%}}[[c({{dependent}})]]&#xA;  }&#xA;  else&#xA;  {&#xA;  TrainData &lt;- {{%DATASET%}}[,{{independent}}]&#xA;  TrainClasses  &lt;- {{%DATASET%}}[,c({{dependent}})]&#xA;  }&#xA;  # define training control&#xA; train_control &lt;- caret::trainControl(method=&quot;LOOCV&quot;)&#xA;  # train the model&#xA;#We pass the parameter prob.model=TRUE to get predicted probabilities for svm, this is required for factor/ordinal and string and not numeric&#xA;  if ( (modelselected == &quot;svmLinear&quot; || modelselected == &quot;svmRadial&quot; ||  modelselected == &quot;svmPoly&quot;  ) &amp;&amp; (class( {{%DATASET%}}[,c({{dependent}})]) ==&quot;ordinal&quot; ||class( {{%DATASET%}}[,c({{dependent}})]) ==&quot;factor&quot; || class( {{%DATASET%}}[,c({{dependent}})]) ==&quot;character&quot; ))&#xA;  {&#xA;    .GlobalEnv${{modelname}} &lt;- caret::train(as.data.frame(TrainData) ,TrainClasses , trControl=train_control, method= &quot;{{ModelType}}&quot;,preProcess=NULL, prob.model=TRUE )&#xA;  }&#xA;  else&#xA;  {&#xA;  .GlobalEnv${{modelname}} &lt;- caret::train(as.data.frame(TrainData) ,TrainClasses , trControl=train_control, method= &quot;{{ModelType}}&quot; )&#xA;  }&#xA;&#xA;&#xA;#We generate the confusion matrix and accuract statistics only for dependent variables of class factor, character and ordered&#xA; if (class(TrainClasses) ==&quot;factor&quot; || class(TrainClasses) ==&quot;ordered&quot; || class(TrainClasses) ==&quot;character&quot;)&#xA; {&#xA; if (!is.null(.GlobalEnv${{modelname}}))&#xA; {&#xA;   #Compute predicted values&#xA;   predictedValues =predict(.GlobalEnv${{modelname}})&#xA;   if (class(TrainClasses) ==&quot;character&quot;)&#xA;   {&#xA;    TrainClasses =as.factor(TrainClasses)&#xA;   }&#xA;   BSkyConfusionMatrixTrain (predictions =predictedValues, reference =TrainClasses)&#xA;  }&#xA;  }&#xA;## save dependent independent var with model &#xA;   attr(.GlobalEnv${{modelname}},&quot;depvar&quot;)=&quot;{{dependent}}&quot;&#xA;   attr(.GlobalEnv${{modelname}},&quot;indepvar&quot;)=&quot;{{independent}}&quot;&#xA;&#xA;   # summarize results&#xA;   print(.GlobalEnv${{modelname}} )&#xA;}&#xA;  else&#xA;  {&#xA;   cat(&quot;There was an error when training the model&quot;)&#xA;  }&#xA;}&#xA;)" dlgsyntax="{}{{modelname}}=character(0)&#xA;local (&#xA;{&#xA; #get short name for  selected model &#xA; #modelshortname = getModelInitials(&quot;{{modelselected}}&quot;)&#xA; #load package for selected model like nb , rf &#xA; modelselected = &quot;{{ModelType}}&quot; &#xA; success = loadReqModelPackage(modelselected )&#xA; if(success)&#xA; {&#xA;  if (&quot;tbl&quot; %in% class({{%DATASET%}}))&#xA;  {&#xA;  TrainData &lt;- {{%DATASET%}}[,{{independent}}]&#xA;  TrainClasses  &lt;- {{%DATASET%}}[[c({{dependent}})]]&#xA;  }&#xA;  else&#xA;  {&#xA;  TrainData &lt;- {{%DATASET%}}[,{{independent}}]&#xA;  TrainClasses  &lt;- {{%DATASET%}}[,c({{dependent}})]&#xA;  }&#xA;  # define training control&#xA; train_control &lt;- caret::trainControl(method=&quot;LOOCV&quot;)&#xA;  # train the model&#xA;#We pass the parameter prob.model=TRUE to get predicted probabilities for svm, this is required for factor/ordinal and string and not numeric&#xA;  if ( (modelselected == &quot;svmLinear&quot; || modelselected == &quot;svmRadial&quot; ||  modelselected == &quot;svmPoly&quot;  ) &amp;&amp; (class( {{%DATASET%}}[,c({{dependent}})]) ==&quot;ordinal&quot; ||class( {{%DATASET%}}[,c({{dependent}})]) ==&quot;factor&quot; || class( {{%DATASET%}}[,c({{dependent}})]) ==&quot;character&quot; ))&#xA;  {&#xA;    .GlobalEnv${{modelname}} &lt;- caret::train(as.data.frame(TrainData) ,TrainClasses , trControl=train_control, method= &quot;{{ModelType}}&quot;,preProcess=NULL, prob.model=TRUE )&#xA;  }&#xA;  else&#xA;  {&#xA;  .GlobalEnv${{modelname}} &lt;- caret::train(as.data.frame(TrainData) ,TrainClasses , trControl=train_control, method= &quot;{{ModelType}}&quot; )&#xA;  }&#xA;&#xA;&#xA;#We generate the confusion matrix and accuract statistics only for dependent variables of class factor, character and ordered&#xA; if (class(TrainClasses) ==&quot;factor&quot; || class(TrainClasses) ==&quot;ordered&quot; || class(TrainClasses) ==&quot;character&quot;)&#xA; {&#xA; if (!is.null(.GlobalEnv${{modelname}}))&#xA; {&#xA;   #Compute predicted values&#xA;   predictedValues =predict(.GlobalEnv${{modelname}})&#xA;   if (class(TrainClasses) ==&quot;character&quot;)&#xA;   {&#xA;    TrainClasses =as.factor(TrainClasses)&#xA;   }&#xA;   BSkyConfusionMatrixTrain (predictions =predictedValues, reference =TrainClasses)&#xA;  }&#xA;  }&#xA;## save dependent independent var with model &#xA;   attr(.GlobalEnv${{modelname}},&quot;depvar&quot;)=&quot;{{dependent}}&quot;&#xA;   attr(.GlobalEnv${{modelname}},&quot;indepvar&quot;)=&quot;{{independent}}&quot;&#xA;&#xA;   # summarize results&#xA;   print(.GlobalEnv${{modelname}} )&#xA;}&#xA;  else&#xA;  {&#xA;   cat(&quot;There was an error when training the model&quot;)&#xA;  }&#xA;}&#xA;)" Helpfile="{x:Null}" splitProcessing="True" DatasetRequired="True" MenuLocation="{x:Null}" OutputDefinition="{x:Null}" Width="550" Command="False" Height="550" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { font-family: 'Times New Roman'; font-size: 18.6666666666667px; font-weight: bold } .c3 { font-size: 13.3333333333333px } .c4 { -c1-editable: false } .c5 { font-size: 13.3333333333333px; font-style: italic } .c6 { color: Blue; text-decoration: underline } .c7 { font-size: 13.3333333333333px; text-decoration: underline } .c8 { font-size: 13.3333333333333px; background-color: White; text-decoration: underline } .c9 { font-weight: bold } .c10 { border-collapse: collapse; width: 100% } .c11 { width: 12.5% } .c12 { vertical-align: middle } .c13 { border-color: Black; border-style: solid; border-width: thin; padding: 0px 7px; vertical-align: middle } .c14 { font-size: 13.3333333333333px; font-weight: bold; text-decoration: underline } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Model Tuning with Leave One Out Cross Validation&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Leave one our cross validation uses the trainControl and train functions within the caret package (see caret.r-forge.r-project.org) to create a tuned/optimized model.&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;The tuned model is stored in the object of class train with a name that you specified in the textbox control. The parameter values selected for the final tuned model are displayed in the output. The final model with the optimal parameters can be accessed by entering the name specified in the textbox control followed by $finalModel in the BlueSky Statistics syntax editor, for example Model1$finalModel. This is the model we use when you score a dataset.&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&amp;#x200b;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;We generate a confusion matrix and model accuracy statistics for the tuned model. This is done as follows&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;1. We generate predicted values from the training dataset by calling the predict function on the optimized model of class train (Internally predict.train is the function in the caret package that R calls)&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;predictedValues =predict(Enter the model name you specified)&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;2. We compute &amp;#160;accuracy statistics from the confusion matrix generated&lt;br/&gt;caret::confusionMatrix (predictions =predictedValues, reference =model dependent variable)&lt;br/&gt;&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;You can use the final tuned/optimized model to score a dataset. To do so, follow the steps below&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;1. Use the select model class and/or pick the model controls on the top right of main application window (the window that contains the data grid) to find the model you &amp;#160;created (the model name you specified in the model name text box control).&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Note: This is the optimized/tuned model&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&amp;#x200b;&lt;img src=&quot;data:image/png;base64,&quot; class=&quot;c4&quot;/&gt;&amp;#x200b;&amp;#x200b;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;2. Go to the dataset you want to score. Note: The variable names in the dataset to be scored (a.k.a. independent variables) must match the variable names you used in the &amp;#160;dataset to originally create the tuned model. If not an error will display.&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;3. If you are scoring a testing dataset, make sure that the dependent variable name in the test dataset matches the name of the dependent variable used to originally tune the model. If not the confusion matrix and ROC curve will not display.&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;4. Once you have selected the model and the dataset you want to score, click on the score button to score the dataset. You must specify a prefix for the variable that contains the predictions/scores. The predictions and predicted probabilities where applicable are stored as new variables at the end of the dataset. If the prefix specified is AA, the predictions are stored in a variable&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;AA_original dependent variable name_Predictions&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;, the predicted probabilities are stored in a variable&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;AA_Level1_PredictedProbs&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&amp;#x200b;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;For more details on bootstrap resampling refer to&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;a href=&quot;https://machinelearningmastery.com/how-to-estimate-model-accuracy-in-r-using-the-caret-package/&quot; class=&quot;c6&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;https://machinelearningmastery.com/how-to-estimate-model-accuracy-in-r-using-the-caret-package/&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;a href=&quot;https://www.r-project.org/conferences/useR-2013/Tutorials/kuhn/user_caret_2up.pdf&quot; class=&quot;c6&quot;&gt;&lt;span&gt;&lt;span class=&quot;c8&quot;&gt;https://www.r-project.org/conferences/useR-2013/Tutorials/kuhn/user_caret_2up.pdf&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;a href=&quot;http://topepo.github.io/caret/index.html&quot; class=&quot;c6&quot;&gt;http://topepo.github.io/caret/index.html&lt;/a&gt;&amp;#x200b;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&amp;#x200b;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;NOTE: &amp;#160;THE FOLLOWING MODELING ROUTINES REQUIRE INDEPENDENT VARIABLES OF TYPE FACTOR TO BE DUMMY CODED&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;Bayes Ridge Regression&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;XGBBOOST&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;NEURALNET&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;KNN&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;GLMNET&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;DNN&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;MLP&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;SVMLINEAR&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;SVMRADIAL&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;SVMPOLY&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;NEURALNET ONLY SUPPORTS NUMERIC INDEPENDENT VARIABLES ONLY&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;The following models are available for tuning&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&lt;/p&gt;&lt;table class=&quot;c10&quot;&gt;&lt;col class=&quot;c11&quot;/&gt;&lt;col class=&quot;c11&quot;/&gt;&lt;col class=&quot;c11&quot;/&gt;&lt;col class=&quot;c11&quot;/&gt;&lt;col class=&quot;c11&quot;/&gt;&lt;col class=&quot;c11&quot;/&gt;&lt;col class=&quot;c11&quot;/&gt;&lt;col class=&quot;c11&quot;/&gt;&lt;tbody class=&quot;c12&quot;&gt;&lt;tr class=&quot;c12&quot;&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;Model Type&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;Modeling function passed as a parameter to the train function in the caret package&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;Class of the final model&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;R Package&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;Supported classes for dependent variables&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;IMPORTANT NOTES&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;Sample datasets can be found by clicking File&amp;gt;Load Dataset from an R Package&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c12&quot;&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;Adaboost Classification Trees&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;adaboost&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;adaboost&lt;/p&