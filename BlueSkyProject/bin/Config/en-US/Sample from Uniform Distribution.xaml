<BSkyCanvas internalhelpfilename="{x:Null}" Title="Sample from Uniform Distribution" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="{x:Null}" CanExecute="True" PrereqCommandString="{x:Null}" CommandString="base::set.seed({{seedval}})&#xA;&#xA;{{datasetname}} &lt;- as.data.frame(matrix(stats::runif({{nosamp}}*{{noobserv}}, min={{min}}, max={{max}}), ncol={{noobserv}}))&#xA;rownames({{datasetname}}) &lt;- &quot;sample&quot;&#xA;colnames({{datasetname}}) &lt;- paste(&quot;obs&quot;, 1:{{noobserv}}, sep=&quot;&quot;)&#xA;{{datasetname}}&lt;- within({{datasetname}}, &#xA;{  &#xA;if({{sampmean}}) mean &lt;- rowMeans({{datasetname}}[,1:{{noobserv}}]) &#xA; if({{sampsums}}) sum &lt;- rowSums({{datasetname}}[,1:{{noobserv}}])&#xA; if({{sampstddev}}) sd &lt;- apply({{datasetname}}[,1:{{noobserv}}], 1, sd)&#xA;})&#xA;BSkyFormat({{datasetname}}, engNotationSetting=BSkyGetEngNotationSetting(), singleTableOutputHeader=&quot;Uniform Samples&quot;)&#xA;&#xA;BSkyLoadRefreshDataframe({{datasetname}})" dlgsyntax="base::set.seed({{seedval}})&#xA;&#xA;{{datasetname}} &lt;- as.data.frame(matrix(stats::runif({{nosamp}}*{{noobserv}}, min={{min}}, max={{max}}), ncol={{noobserv}}))&#xA;rownames({{datasetname}}) &lt;- &quot;sample&quot;&#xA;colnames({{datasetname}}) &lt;- paste(&quot;obs&quot;, 1:{{noobserv}}, sep=&quot;&quot;)&#xA;{{datasetname}}&lt;- within({{datasetname}}, &#xA;{  &#xA;if({{sampmean}}) mean &lt;- rowMeans({{datasetname}}[,1:{{noobserv}}]) &#xA; if({{sampsums}}) sum &lt;- rowSums({{datasetname}}[,1:{{noobserv}}])&#xA; if({{sampstddev}}) sd &lt;- apply({{datasetname}}[,1:{{noobserv}}], 1, sd)&#xA;})&#xA;BSkyFormat({{datasetname}}, engNotationSetting=BSkyGetEngNotationSetting(), singleTableOutputHeader=&quot;Uniform Samples&quot;)&#xA;&#xA;BSkyLoadRefreshDataframe({{datasetname}})" Helpfile="{x:Null}" splitProcessing="True" DatasetRequired="False" MenuLocation="{x:Null}" OutputDefinition="" Width="250" Command="False" Height="432" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold } .c3 { font-family: 'Courier New' } .c4 { vertical-align: middle } .c5 { vertical-align: top } .c6 { margin: 0px 0px 12px } .c7 { font-style: italic } .c8 { margin: 0px 0px 10px; text-align: center } .c9 { color: Blue; text-decoration: underline; font-family: 'Courier New' } .c10 { font-weight: bold; font-family: 'Courier New','Courier'; color: #666666 } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&amp;#x200b;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;These functions provide information about the uniform distribution on the interval from&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;min&lt;/span&gt;&lt;/span&gt;&amp;#160;to&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;max&lt;/span&gt;&lt;/span&gt;.&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dunif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the density,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;punif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the distribution function&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qunif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the quantile function and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;runif&lt;/span&gt;&lt;/span&gt;&amp;#160;generates random deviates.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Usage&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dunif(x, min = 0, max = 1, log = FALSE)&lt;br/&gt;punif(q, min = 0, max = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;qunif(p, min = 0, max = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;runif(n, min = 0, max = 1)&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c4&quot;&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;x, q&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of quantiles.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of probabilities.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;number of observations. If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;length(n) &amp;gt; 1&lt;/span&gt;&lt;/span&gt;, the length is taken to be the number required.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;min, max&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;lower and upper limits of the distribution. Must be finite.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;log, log.p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE, probabilities p are given as log(p).&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;lower.tail&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE (default), probabilities are&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X ≤ x]&lt;/span&gt;&lt;/span&gt;, otherwise,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X &amp;gt; x]&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Details&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;min&lt;/span&gt;&lt;/span&gt;&amp;#160;or&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;max&lt;/span&gt;&lt;/span&gt;&amp;#160;are not specified they assume the default values of&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;0&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;1&lt;/span&gt;&lt;/span&gt;&amp;#160;respectively.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The uniform distribution has density&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;f(x) = 1/(max-min)&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;min ≤ x ≤ max&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;For the case of&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;u := min == max&lt;/span&gt;&lt;/span&gt;, the limit case of&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;X == u&lt;/span&gt;&lt;/span&gt;&amp;#160;is assumed, although there is no density in that case and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dunif&lt;/span&gt;&lt;/span&gt;&amp;#160;will return&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;NaN&lt;/span&gt;&lt;/span&gt;&amp;#160;(the error condition).&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;runif&lt;/span&gt;&lt;/span&gt;&amp;#160;will not generate either of the extreme values unless&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;max = min&lt;/span&gt;&lt;/span&gt;&amp;#160;or&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;max-min&lt;/span&gt;&lt;/span&gt;&amp;#160;is small compared to&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;min&lt;/span&gt;&lt;/span&gt;, and in particular not for the default arguments.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Value&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dunif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the density,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;punif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the distribution function,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qunif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the quantile function, and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;runif&lt;/span&gt;&lt;/span&gt;&amp;#160;generates random deviates.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The length of the result is determined by&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;runif&lt;/span&gt;&lt;/span&gt;, and is the maximum of the lengths of the numerical arguments for the other functions.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The numerical arguments other than&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;are recycled to the length of the result. Only the first elements of the logical arguments are used.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Note&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;The characteristics of output from pseudo-random number generators (such as precision and periodicity) vary widely. See&amp;#160;&lt;a href=&quot;http://127.0.0.1:21039/library/stats/help/.Random.seed&quot; class=&quot;c9&quot;&gt;.Random.seed&lt;/a&gt;&amp;#160;for more information on&amp;#160;&lt;span&gt;&lt;span class=&quot;c10&quot;&gt;R&lt;/span&gt;&lt;/span&gt;'s random number generation algorithms.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(runif, package=stats)" Background="#FFFFFFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"><BSkySpinnerCtrl Syntax="{x:Null}" Enabled="True" Name="nosamp" Text="1" Step="1" Width="83" Height="22" Left="6" Top="213" CanExecute="True" IsEnabled="True" av:Canvas.Left="6" av:Canvas.Top="213"></BSkySpinnerCtrl><BSkySpinnerCtrl Syntax="{x:Null}" Enabled="True" Name="noobserv" Text="100" Step="1" Width="83" Height="22" Left="6" Top="266" CanExecute="True" IsEnabled="True" av:Canvas.Left="6" av:Canvas.Top="266"></BSkySpinnerCtrl><BSkySpinnerCtrl Syntax="{x:Null}" Enabled="True" Name="seedval" Text="12345" Step="1" Width="83" Height="22" Left="6" Top="318" CanExecute="True" IsEnabled="True" av:Canvas.Left="6" av:Canvas.Top="318"></BSkySpinnerCtrl><BSkyLabel Name="lbl1" Text="Enter name for dataset" Width="142" Height="26" Left="2" Top="5" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="2" av:Canvas.Top="5">Enter name for dataset</BSkyLabel><BSkyTextBox PrefixTxt="{x:Null}" OverWriteExistingVariables="False" OverwriteSettings="PromptBeforeOverwritingDatasets" Name="datasetname" Width="131" Height="30" Left="6" Top="31" SubstituteSettings="TextAsIs" CanExecute="True" Enabled="True" Syntax="%%VALUE%%" TextWrapping="Wrap" AcceptsTab="True" VerticalScrollBarVisibility="Auto" FontFamily="Segoe UI" FontSize="12" VerticalContentAlignment="Center" Template="{av:DynamicResource TextBoxBaseControlTemplate}" AllowDrop="True" IsEnabled="True" av:Canvas.Left="6" av:Canvas.Top="31"><BSkyTextBox.TextChangedBehaviour><BehaviourCollection Capacity="4"><Behaviour><Behaviour.Condition><Condition PropertyName="Text" Value="{x:Null}" Operator="isNullOrEmpty" /></Behaviour.Condition><Behaviour.Setters><PropertySettersCollection Capacity="4"><PropertyS