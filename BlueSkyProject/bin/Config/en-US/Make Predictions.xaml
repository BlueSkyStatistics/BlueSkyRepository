<BSkyCanvas internalhelpfilename="{assembly:Null}" Title="Make Predictions" ModelClasses="NaiveBayes; randomForest; lm; glm; rpart; multinom; nnet; polr;ksvm;blasso;knn3;real_adaboost;adaboost" StatusTextBoxName="msgbox" customsyntax="{assembly:Null}" EditableComboBoxName="{assembly:Null}" RPackages="caret; stats;ROCR;dplyr" CanExecute="True" PrereqCommandString="predictPrerequisite('{{%MODEL%}}', '{{%DATASET%}}')" CommandString="local(&#xA;{&#xA;#Run predict&#xA;BSkyPredictions &lt;- BSkyPredictnew(modelname='{{%MODEL%}}', prefix='{{colname}}', datasetname='{{%DATASET%}}')&#xA;&#xA;#Generate ConfusionMatrix only if it passes above condition and if the 'confusion matrix checkbox' is checked&#xA;BSkyTmp &lt;- BSkyConfusionMatrix(modelname='{{%MODEL%}}', showConfusionMatrix={{confusioncheck}}, predictions=BSkyPredictions[[1]] , datasetname='{{%DATASET%}}')&#xA;&#xA;ROC=BSkyPredictions[[4]]&#xA;if (ROC)&#xA;{&#xA;   if( class(BSkyPredictions[[3]] )  == 'logical' )&#xA;    {&#xA;        BSkyPredictions[[3]] = as.factor( BSkyPredictions[[3]] )&#xA;    }&#xA;&#xA; if( getModelClass(&quot;{{%MODEL%}}&quot;)  == 'glm' )&#xA;{&#xA;if (eval( parse(text=paste (&quot;family(&quot; ,&quot;{{%MODEL%}}&quot;, &quot;)$family&quot;, sep='', collapse='') )) ==&quot;binomial&quot; )&#xA;{&#xA;   BSkyPredictions[[3]] = as.factor( BSkyPredictions[[3]] )&#xA;}&#xA;}&#xA;&#xA; if ({{roctable}})&#xA;{&#xA;results &lt;-createROCTable(predictedprobs =BSkyPredictions[[2]],dependentvariable =BSkyPredictions[[3]], modelname=&quot;{{%MODEL%}}&quot;,datasetname =&quot;{{%DATASET%}}&quot;)&#xA;BSkyFormat(results, singleTableOutputHeader='ROC Table')&#xA;}&#xA;&#xA;    pr &lt;- prediction(BSkyPredictions[[2]], BSkyPredictions[[3]])&#xA;    prf &lt;- performance(pr, measure = &quot;tpr&quot;, x.measure = &quot;fpr&quot;)&#xA;    plot(prf, main = &quot;ROC Curve&quot;)&#xA;    auc &lt;- performance(pr, measure = &quot;auc&quot;)&#xA;    auc &lt;- auc@y.values[[1]]&#xA;    cat( paste(&quot;The area under the curve (AUC) is&quot;,auc,sep=&quot; &quot;))&#xA;}&#xA;}&#xA;)&#xA;#Refresh dataset&#xA;BSkyLoadRefreshDataframe({{%DATASET%}})&#xA;" dlgsyntax="local(&#xA;{&#xA;#Run predict&#xA;BSkyPredictions &lt;- BSkyPredictnew(modelname='{{%MODEL%}}', prefix='{{colname}}', datasetname='{{%DATASET%}}')&#xA;&#xA;#Generate ConfusionMatrix only if it passes above condition and if the 'confusion matrix checkbox' is checked&#xA;BSkyTmp &lt;- BSkyConfusionMatrix(modelname='{{%MODEL%}}', showConfusionMatrix={{confusioncheck}}, predictions=BSkyPredictions[[1]] , datasetname='{{%DATASET%}}')&#xA;&#xA;ROC=BSkyPredictions[[4]]&#xA;if (ROC)&#xA;{&#xA;   if( class(BSkyPredictions[[3]] )  == 'logical' )&#xA;    {&#xA;        BSkyPredictions[[3]] = as.factor( BSkyPredictions[[3]] )&#xA;    }&#xA;&#xA; if( getModelClass(&quot;{{%MODEL%}}&quot;)  == 'glm' )&#xA;{&#xA;if (eval( parse(text=paste (&quot;family(&quot; ,&quot;{{%MODEL%}}&quot;, &quot;)$family&quot;, sep='', collapse='') )) ==&quot;binomial&quot; )&#xA;{&#xA;   BSkyPredictions[[3]] = as.factor( BSkyPredictions[[3]] )&#xA;}&#xA;}&#xA;&#xA; if ({{roctable}})&#xA;{&#xA;results &lt;-createROCTable(predictedprobs =BSkyPredictions[[2]],dependentvariable =BSkyPredictions[[3]], modelname=&quot;{{%MODEL%}}&quot;,datasetname =&quot;{{%DATASET%}}&quot;)&#xA;BSkyFormat(results, singleTableOutputHeader='ROC Table')&#xA;}&#xA;&#xA;    pr &lt;- prediction(BSkyPredictions[[2]], BSkyPredictions[[3]])&#xA;    prf &lt;- performance(pr, measure = &quot;tpr&quot;, x.measure = &quot;fpr&quot;)&#xA;    plot(prf, main = &quot;ROC Curve&quot;)&#xA;    auc &lt;- performance(pr, measure = &quot;auc&quot;)&#xA;    auc &lt;- auc@y.values[[1]]&#xA;    cat( paste(&quot;The area under the curve (AUC) is&quot;,auc,sep=&quot; &quot;))&#xA;}&#xA;}&#xA;)&#xA;#Refresh dataset&#xA;BSkyLoadRefreshDataframe({{%DATASET%}})&#xA;" Helpfile="{assembly:Null}" splitProcessing="False" MenuLocation="{assembly:Null}" OutputDefinition="{assembly:Null}" Width="565" Command="False" Height="340" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { font-weight: bold; font-size: 14.6666660308838px; line-height: 1.14999997615814; font-family: 'Arial' } .c3 { font-weight: normal; font-size: 14.6666660308838px; line-height: 1.14999997615814; font-family: 'Arial' } .c4 { margin: 0px 0px 13.3333339691162px; line-height: 1.14999997615814; font-size: 11px; font-family: 'Arial' } .c5 { font-weight: normal; font-size: 14.6666660308838px } .c6 { font-weight: bold; font-size: 14.6666660308838px } .c7 { margin: 0px 0px 10.0000009536743px; font-size: 11px; font-family: 'Arial' } .c8 { font-weight: bold; font-size: 14.6666660308838px; font-family: 'Arial' } .c9 { font-weight: normal; font-size: 14.6666660308838px; font-family: 'Arial' } .c10 { margin: 0px 0px 10px; font-size: 11px; font-family: 'Arial' } .c11 { font-weight: bold; font-size: 14.6666660308838px; font-family: 'Arial'; text-decoration: underline } .c12 { font-weight: normal; font-size: 14.6666660308838px; font-family: 'Arial'; text-decoration:  } .c13 { font-weight: bold; font-size: 14.6666660308838px; font-family: 'Arial'; text-decoration:  } .c14 { margin: 0px 0px 10.0000009536743px; line-height: 1.14999997615814; font-size: 11px; font-family: 'Arial' } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&lt;span class=&quot;c2&quot;&gt;Description:&lt;/span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c5&quot;&gt;predict is a generic function for predictions from the results of various model fitting functions. The function invokes particular methods which depend on the class of the first argument.&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c6&quot;&gt;Usage:&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c5&quot;&gt;BSkyPredict(modelname, prefix, datasetname)&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c6&quot;&gt;Arguments:&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c5&quot;&gt;modelname : a model object for which prediction is desired.&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c5&quot;&gt;prefix : &amp;#160;prefix string that will be used as a name for new column. If there are mulitple columns then this string is prepended to the each column name.&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c7&quot;&gt;&lt;span class=&quot;c5&quot;&gt;datasetname: is the current datasetname using which we want to make predictions.&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c7&quot;&gt;&lt;span class=&quot;c5&quot;&gt;&amp;#160;&lt;/span&gt;&lt;span class=&quot;c6&quot;&gt;Package: BlueSky, stats&lt;/span&gt;&lt;span class=&quot;c8&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;span class=&quot;c9&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c10&quot;&gt;&lt;span class=&quot;c9&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c7&quot;&gt;&lt;span class=&quot;c11&quot;&gt;NOTE:&amp;#160;&lt;/span&gt;&lt;span class=&quot;c12&quot;&gt;Confusion Matrix will also be generated where ever applicable if user selected the checkbox option. Here are the details reated to confusion matrix:&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c13&quot;&gt;Description:&lt;/span&gt;&lt;span class=&quot;c12&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c12&quot;&gt;Calculates a cross-tabulation of observed and predicted classes with associated statistics.&amp;#160;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c13&quot;&gt;Usage:&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c12&quot;&gt;BSkyConfusionMartix(modelname,showCofusionMatrix,predictions,datasetname)&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c13&quot;&gt;Arguments:&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c12&quot;&gt;modelname : a model object for which confusion matrix is desired.&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c12&quot;&gt;showCofusionMatrix: &amp;#160;logical, if TRUE the confusion matrix is generated (if it applies), if FALSE, confusion matrix is not generated.&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c12&quot;&gt;predictions : an object that is returned as a result of pridict() call.&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c7&quot;&gt;&lt;span class=&quot;c12&quot;&gt;datasetname: is the current datasetname using which we want to make predictions.&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c7&quot;&gt;&lt;span class=&quot;c12&quot;&gt;&amp;#160;&lt;/span&gt;&lt;span class=&quot;c13&quot;&gt;Package: BlueSky, caret&lt;/span&gt;&lt;span class=&quot;c13&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;span class=&quot;c12&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c14&quot;&gt;&lt;span class=&quot;c13&quot;&gt;&amp;#160;Other:&amp;#160;&lt;/span&gt;&lt;span class=&quot;c12&quot;&gt;Click the R Help button to get detailed R help&lt;/span&gt;&lt;span class=&quot;c12&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;span class=&quot;c12&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c4&quot;&gt;&lt;span class=&quot;c12&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(predict, package=stats)" Background="#FFEEEFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:assembly="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"><BSkyLabel Name="lbl3" Text="Save predicted values and supporting statistics." Width="270" Height="31" Left="7" Top="220" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="7" av:Canvas.Top="220">Save predict