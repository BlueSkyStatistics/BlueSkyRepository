<BSkyCanvas internalhelpfilename="{x:Null}" Title="Show Rules" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="arules" CanExecute="True" PrereqCommandString="{x:Null}" CommandString="local(&#xA;{&#xA;## Sorting&#xA;if( {{dosortingchk}} )&#xA;{&#xA;tempsup&lt;-c()&#xA;if({{suppchk}})&#xA;{&#xA;tempsup = c('support')&#xA;}&#xA;tempconf&lt;-c()&#xA;if({{confchk}})&#xA;{&#xA;tempconf = c('confidence')&#xA;}&#xA;templift&lt;-c()&#xA;if({{liftchk}})&#xA;{&#xA;templift = c('lift')&#xA;}&#xA;&#xA;{{rulename}}  &lt;- sort( {{rulename}} , decreasing={{ordergrp}}, by=c(tempsup, tempconf, templift) )&#xA;}&#xA;&#xA;# Extract LHS&#xA;lhsitems &lt;- as(lhs({{rulename}}), &quot;list&quot;)&#xA;&#xA;# Extract RHS&#xA;rhsitems &lt;- as(rhs({{rulename}}), &quot;list&quot;)&#xA;&#xA;# Extract QUALITY( that has support, confidence and lift)&#xA;qual &lt;- as(quality({{rulename}}), &quot;list&quot;)&#xA;&#xA;# support&#xA;supp &lt;- qual[[1]] &#xA;# confidence&#xA;conf &lt;- qual[[2]] &#xA;# lift&#xA;lift &lt;- qual[[3]] &#xA;&#xA;#checking rule count&#xA;recs &lt;- length(supp)&#xA;if(recs&lt;1)&#xA;{&#xA;print(&quot;No Rules to Display&quot;)&#xA;return&#xA;}&#xA;&#xA;fromidx = 1&#xA;toidx= min( {{rulecount}}, recs)&#xA;&#xA;if({{fromto}}{{numofrule}})&#xA;{&#xA;fromidx = min( recs-1, {{from}})&#xA;toidx = min( recs, {{to}})&#xA;}&#xA;&#xA;#collecting all lists in one list&#xA;megalist=list(as.character(lhsitems[fromidx:toidx]), as.character(rhsitems[fromidx:toidx]), supp[fromidx:toidx], conf[fromidx:toidx], lift[fromidx:toidx] )&#xA;&#xA;#creating data.frame out of the list&#xA;DF1 &lt;- do.call(cbind, megalist) %&gt;% as.data.frame&#xA;&#xA;#changing V1, V2, V3... col names&#xA;names(DF1) &lt;- c(&quot;LHS&quot;, &quot;RHS&quot;, &quot;Support&quot;, &quot;Confidence&quot;, &quot;Lift&quot;)&#xA;&#xA;# Formatted printing&#xA;BSkyFormat(DF1)&#xA;}&#xA;)" dlgsyntax="local(&#xA;{&#xA;## Sorting&#xA;if( {{dosortingchk}} )&#xA;{&#xA;tempsup&lt;-c()&#xA;if({{suppchk}})&#xA;{&#xA;tempsup = c('support')&#xA;}&#xA;tempconf&lt;-c()&#xA;if({{confchk}})&#xA;{&#xA;tempconf = c('confidence')&#xA;}&#xA;templift&lt;-c()&#xA;if({{liftchk}})&#xA;{&#xA;templift = c('lift')&#xA;}&#xA;&#xA;{{rulename}}  &lt;- sort( {{rulename}} , decreasing={{ordergrp}}, by=c(tempsup, tempconf, templift) )&#xA;}&#xA;&#xA;# Extract LHS&#xA;lhsitems &lt;- as(lhs({{rulename}}), &quot;list&quot;)&#xA;&#xA;# Extract RHS&#xA;rhsitems &lt;- as(rhs({{rulename}}), &quot;list&quot;)&#xA;&#xA;# Extract QUALITY( that has support, confidence and lift)&#xA;qual &lt;- as(quality({{rulename}}), &quot;list&quot;)&#xA;&#xA;# support&#xA;supp &lt;- qual[[1]] &#xA;# confidence&#xA;conf &lt;- qual[[2]] &#xA;# lift&#xA;lift &lt;- qual[[3]] &#xA;&#xA;#checking rule count&#xA;recs &lt;- length(supp)&#xA;if(recs&lt;1)&#xA;{&#xA;print(&quot;No Rules to Display&quot;)&#xA;return&#xA;}&#xA;&#xA;fromidx = 1&#xA;toidx= min( {{rulecount}}, recs)&#xA;&#xA;if({{fromto}}{{numofrule}})&#xA;{&#xA;fromidx = min( recs-1, {{from}})&#xA;toidx = min( recs, {{to}})&#xA;}&#xA;&#xA;#collecting all lists in one list&#xA;megalist=list(as.character(lhsitems[fromidx:toidx]), as.character(rhsitems[fromidx:toidx]), supp[fromidx:toidx], conf[fromidx:toidx], lift[fromidx:toidx] )&#xA;&#xA;#creating data.frame out of the list&#xA;DF1 &lt;- do.call(cbind, megalist) %&gt;% as.data.frame&#xA;&#xA;#changing V1, V2, V3... col names&#xA;names(DF1) &lt;- c(&quot;LHS&quot;, &quot;RHS&quot;, &quot;Support&quot;, &quot;Confidence&quot;, &quot;Lift&quot;)&#xA;&#xA;# Formatted printing&#xA;BSkyFormat(DF1)&#xA;}&#xA;)" Helpfile="{x:Null}" splitProcessing="True" MenuLocation="{x:Null}" OutputDefinition="" Width="350" Command="False" Height="280" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { font-weight: bold; font-size: 12.87px; text-decoration:  } .c3 { font-size: 12.87px; font-weight: bold; text-decoration: underline } .c4 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold } .c5 { font-family: 'Courier New' } .c6 { vertical-align: middle } .c7 { vertical-align: top } .c8 { margin: 0px 0px 12px } .c9 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold; text-decoration: underline } .c10 { margin: 0px 0px 11px; font-size: 12px; font-weight: bold } .c11 { margin: 0px 0px 11px; font-size: 12.87px } .c12 { font-size: 12.87px; font-weight: bold } .c13 { color: Blue; text-decoration: underline; font-family: 'Courier New' } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Package : arules&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;1. &amp;#160;inspect&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;Provides the generic function&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;inspect&lt;/span&gt;&lt;/span&gt;&amp;#160;and S4 methods to display associations and transactions plus additional information formatted for online inspection.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;Usage&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;inspect(x, ...)&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c6&quot;&gt;&lt;tr class=&quot;c7&quot;&gt;&lt;td class=&quot;c7&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;x&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c7&quot;&gt;&lt;p class=&quot;c8&quot;&gt;a set of associations or transactions or an itemMatrix.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c7&quot;&gt;&lt;td class=&quot;c7&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;...&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c7&quot;&gt;&lt;p class=&quot;c8&quot;&gt;additional arguments can be used to customize the output:&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;setStart&lt;/span&gt;&lt;/span&gt;,&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;setEnd&lt;/span&gt;&lt;/span&gt;,&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;itemSep&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;ruleSep&lt;/span&gt;&lt;/span&gt;. Items are printed only one per line in case the output lines get very long. This can also be directly controlled using&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;linebreak&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;2. sort&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c10&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span&gt;&lt;span class=&quot;c11&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;p class=&quot;c8&quot;&gt;Provides the method&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;sort&lt;/span&gt;&lt;/span&gt;&amp;#160;to sort elements in class&amp;#160;&lt;a href=&quot;http://127.0.0.1:23574/library/arules/help/associations-class&quot; class=&quot;c13&quot;&gt;associations&lt;/a&gt;&amp;#160;(e.g., itemsets or rules) according to the value of measures stored in the association's slot&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;quality&lt;/span&gt;&lt;/span&gt;&amp;#160;(e.g., support).&lt;/p&gt;&lt;div&gt;&lt;span class=&quot;c4&quot;&gt;Usage&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;## S4 method for signature 'associations'&lt;br/&gt;sort(x, decreasing = TRUE, na.last = NA, &lt;br/&gt; &amp;#160;&amp;#160;&amp;#160;by = &amp;quot;support&amp;quot;, order = FALSE, ...)&lt;br/&gt;&lt;br/&gt;## S4 method for signature 'associations'&lt;br/&gt;head(x, n = 6L, by = NULL, decreasing = TRUE, ...)&lt;br/&gt;## S4 method for signature 'associations'&lt;br/&gt;tail(x, n = 6L, by = NULL, decreasing = TRUE, ...)&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span class=&quot;c4&quot;&gt;Arguments&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c6&quot;&gt;&lt;tr class=&quot;c7&quot;&gt;&lt;td class=&quot;c7&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;x&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c7&quot;&gt;&lt;p class=&quot;c8&quot;&gt;an object to be sorted.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c7&quot;&gt;&lt;td class=&quot;c7&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;decreasing&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c7&quot;&gt;&lt;p class=&quot;c8&quot;&gt;a logical. Should the sort be increasing or decreasing? (default is decreasing)&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c7&quot;&gt;&lt;td class=&quot;c7&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;na.last&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c7&quot;&gt;&lt;p class=&quot;c8&quot;&gt;na.last is not supported for associations. NAs are always put last.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c7&quot;&gt;&lt;td class=&quot;c7&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;by&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c7&quot;&gt;&lt;p class=&quot;c8&quot;&gt;a character string specifying the quality measure stored in&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;x&lt;/span&gt;&lt;/span&gt;&amp;#160;to be used to sort&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;x&lt;/span&gt;&lt;/span&gt;. If a vector of character strings is specified then the additional strings are used to sort&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;x&lt;/span&gt;&lt;/span&gt;&amp;#160;in case of ties.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c7&quot;&gt;&lt;td class=&quot;c7&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;order&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c7&quot;&gt;&lt;p class=&quot;c8&quot;&gt;should a order vector be returned instead of the sorted associations?&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c7&quot;&gt;&lt;td class=&quot;c7&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c7&quot;&gt;&lt;p class=&quot;c8&quot;&gt;a single integer indicating the number of associations returned.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c7&quot;&gt;&lt;td class=&quot;c7&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;...&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c7&quot;&gt;&lt;p class=&quot;c8&quot;&gt;Further arguments are ignored.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div&gt;&lt;span class=&quot;c4&quot;&gt;Details&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;sort&lt;/span&gt;&lt;/span&gt;&amp;#160;is relatively slow for large sets of associations since it has to copy and rearrange a large data structure. Note that sorting creates a second copy of the set of associations which can be slow and memory consuming for large sets. With&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;order = TRUE&lt;/span&gt;&lt;/span&gt;&amp;#160;a integer vector with the order is returned instead of the reordered associations.&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;If only the top&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;associations are needed then&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;head&lt;/span&gt;&lt;/span&gt;&amp;#160;using&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;by&lt;/span&gt;&lt;/span&gt;&amp;#160;performs this faster than calling&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;sort&lt;/span&gt;&lt;/span&gt;&amp;#160;and then&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;head&lt;/span&gt;&lt;/span&gt;&amp;#160;since it does it without copying and rearranging all the data.&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;tail&lt;/span&gt;&lt;/span&gt;&amp;#160;works in the same way.&lt;/p&gt;&lt;div&gt;&lt;span class=&quot;c4&quot;&gt;Value&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c8&quot;&gt;An object of the same class as&amp;#160;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;x&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/div&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(inspect, package=&quot;arules&quot;)" Background="#FFFFFFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"><BSkyLabel Name="l1" Text="Enter an existing rules object" Width="164" Height="29" Left="5" Top="10" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="5" av:Canvas.Top="10">Enter an existing rules object</BSkyLabel><BSkyTextBox PrefixTxt="{x:Null}" OverWriteExistingVariables="False" OverwriteSettings="DontPrompt" Name="rulename" Width="169" Height="30" Left="173" Top="10" SubstituteSettings="TextAsIs" CanExecute="True" Enabled="True" Syntax="%%VALUE%%" TextWrapping="Wrap" AcceptsTab="True" VerticalScrollBarVisibility="Auto" FontFamily="Segoe UI" FontSize="12" VerticalContentAlignment="Center" Template="{av:DynamicResource TextBoxBaseControlTemplate}" AllowDrop="True" IsEnabled="True" av:Canvas.Left="173" av:Canvas.Top="10"><BSkyTextBox.TextChangedBehaviour><BehaviourCollection Capacity="4"><Behaviour><Behaviour.Condition><Condition PropertyName="Text" Value="{x:Null}" Operator="ValidString" /></Behaviour.Condition><Behaviour.Setters><PropertySettersCollection Capacity="4"><PropertySetter