<BSkyCanvas internalhelpfilename="{x:Null}" Title="Hypergeometric Probabilities" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="{x:Null}" CanExecute="True" PrereqCommandString="{x:Null}" CommandString="local(&#xA;{&#xA;.x &lt;- 0:1&#xA;if({{plotProb}}{{plotDistr}})&#xA;{&#xA;&#xA;  RcmdrMisc::plotDistr(.x, stats::dhyper(.x, m={{m}}, n= {{n}} , k={{k}}), xlab=&quot;Number of White Balls in Sample&quot;, &#xA;  ylab=&quot;Probability Mass&quot;, &#xA;  main=&quot;Hypergeometric Distribution:  m={{m}}, n= {{n}} , k={{k}}&quot;,   discrete=TRUE)&#xA;}&#xA;else&#xA;{&#xA;RcmdrMisc::plotDistr(.x, stats::phyper(.x, m={{m}}, n= {{n}} , k={{k}}), xlab=&quot;Number of White Balls in Sample&quot;,&#xA;  ylab=&quot;Cumulative Probability&quot;, &#xA;main=&quot;Hypergeometric Distribution:  m={{m}}, n= {{n}} , k={{k}}&quot;,   discrete=TRUE, cdf=TRUE)&#xA;}&#xA;&#xA;}&#xA;)" dlgsyntax="local(&#xA;{&#xA;.x &lt;- 0:1&#xA;if({{plotProb}}{{plotDistr}})&#xA;{&#xA;&#xA;  RcmdrMisc::plotDistr(.x, stats::dhyper(.x, m={{m}}, n= {{n}} , k={{k}}), xlab=&quot;Number of White Balls in Sample&quot;, &#xA;  ylab=&quot;Probability Mass&quot;, &#xA;  main=&quot;Hypergeometric Distribution:  m={{m}}, n= {{n}} , k={{k}}&quot;,   discrete=TRUE)&#xA;}&#xA;else&#xA;{&#xA;RcmdrMisc::plotDistr(.x, stats::phyper(.x, m={{m}}, n= {{n}} , k={{k}}), xlab=&quot;Number of White Balls in Sample&quot;,&#xA;  ylab=&quot;Cumulative Probability&quot;, &#xA;main=&quot;Hypergeometric Distribution:  m={{m}}, n= {{n}} , k={{k}}&quot;,   discrete=TRUE, cdf=TRUE)&#xA;}&#xA;&#xA;}&#xA;)" Helpfile="{x:Null}" splitProcessing="True" DatasetRequired="False" MenuLocation="{x:Null}" OutputDefinition="{x:Null}" Width="250" Command="False" Height="230" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold } .c3 { font-family: 'Courier New' } .c4 { vertical-align: middle } .c5 { vertical-align: top } .c6 { margin: 0px 0px 12px } .c7 { font-style: italic } .c8 { margin: 0px 0px 10px; text-align: center } .c9 { color: Blue; text-decoration: underline; font-family: 'Courier New' } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&lt;/p&gt;&lt;div&gt;&lt;span class=&quot;c2&quot;&gt;Description&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;Density, distribution function, quantile function and random generation for the hypergeometric distribution.&lt;/p&gt;&lt;div&gt;&lt;span class=&quot;c2&quot;&gt;Usage&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dhyper(x, m, n, k, log = FALSE)&lt;br/&gt;phyper(q, m, n, k, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;qhyper(p, m, n, k, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;rhyper(nn, m, n, k)&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span class=&quot;c2&quot;&gt;Arguments&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c4&quot;&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;x, q&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of quantiles representing the number of white balls drawn without replacement from an urn which contains both black and white balls.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;m&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;the number of white balls in the urn.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;the number of black balls in the urn.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;k&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;the number of balls drawn from the urn.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;probability, it must be between 0 and 1.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;nn&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;number of observations. If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;length(nn) &amp;gt; 1&lt;/span&gt;&lt;/span&gt;, the length is taken to be the number required.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;log, log.p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE, probabilities p are given as log(p).&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;lower.tail&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE (default), probabilities are&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X ≤ x]&lt;/span&gt;&lt;/span&gt;, otherwise,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X &amp;gt; x]&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div&gt;&lt;span class=&quot;c2&quot;&gt;Details&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;The hypergeometric distribution is used for sampling&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;without&lt;/span&gt;&lt;/span&gt;&amp;#160;replacement. The density of this distribution with parameters&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;m&lt;/span&gt;&lt;/span&gt;,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;k&lt;/span&gt;&lt;/span&gt;&amp;#160;(named&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;Np&lt;/span&gt;&lt;/span&gt;,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;N-Np&lt;/span&gt;&lt;/span&gt;, and&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;n&lt;/span&gt;&lt;/span&gt;, respectively in the reference below) is given by&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;p(x) = choose(m, x) choose(n, k-x) / choose(m+n, k)&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;x = 0, …, k&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;Note that&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;p(x)&lt;/span&gt;&lt;/span&gt;&amp;#160;is non-zero only for&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;max(0, k-n) &amp;lt;= x &amp;lt;= min(k, m)&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;With&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;p := m/(m+n)&lt;/span&gt;&lt;/span&gt;&amp;#160;(hence&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;Np = N \times p&lt;/span&gt;&lt;/span&gt;&amp;#160;in the reference's notation), the first two moments are mean&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;E[X] = μ = k p&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;and variance&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;Var(X) = k p (1 - p) * (m+n-k)/(m+n-1),&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;which shows the closeness to the Binomial&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;(k,p)&lt;/span&gt;&lt;/span&gt;&amp;#160;(where the hypergeometric has smaller variance unless&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;k = 1&lt;/span&gt;&lt;/span&gt;).&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The quantile is defined as the smallest value&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;x&lt;/span&gt;&lt;/span&gt;&amp;#160;such that&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;F(x) ≥ p&lt;/span&gt;&lt;/span&gt;, where&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;F&lt;/span&gt;&lt;/span&gt;&amp;#160;is the distribution function.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;If one of&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;m, n, k&lt;/span&gt;&lt;/span&gt;, exceeds&amp;#160;&lt;a href=&quot;http://127.0.0.1:18258/library/stats/help/.Machine&quot; class=&quot;c9&quot;&gt;.Machine&lt;/a&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;$integer.max&lt;/span&gt;&lt;/span&gt;, currently the equivalent of&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qhyper(runif(nn), m,n,k)&lt;/span&gt;&lt;/span&gt;&amp;#160;is used, when a binomial approximation may be considerably more efficient.&lt;/p&gt;&lt;div&gt;&lt;span class=&quot;c2&quot;&gt;Value&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dhyper&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the density,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;phyper&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the distribution function,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qhyper&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the quantile function, and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rhyper&lt;/span&gt;&lt;/span&gt;&amp;#160;generates random deviates.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;Invalid arguments will result in return value&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;NaN&lt;/span&gt;&lt;/span&gt;, with a warning.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The length of the result is determined by&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rhyper&lt;/span&gt;&lt;/span&gt;, and is the maximum of the lengths of the numerical arguments for the other functions.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The numerical arguments other than&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;are recycled to the length of the result. Only the first elements of the logical arguments are used.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(dhyper, package=stats)" Background="#FFEEEFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"><BSkyLabel Name="mnlbl" Text="m (number of white balls in the urn)" Width="208" Height="41" Left="2" Top="2" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="2" av:Canvas.Top="2">m (number of white balls in the urn)</BSkyLabel><BSkyTextBox PrefixTxt="{x:Null}" OverWriteExistingVariables="False" OverwriteSettings="DontPrompt" Name="m" Width="135" Height="30" Left="6" Top="26" SubstituteSettings="TextAsIs" CanExecute="True" Enabled="True" Syntax="%%VALUE%%" TextWrapping="Wrap" AcceptsTab="True" VerticalScrollBarVisibility="Auto" FontFamily="Segoe UI" FontSize="12" VerticalContentAlignment="Center" Template="{av:DynamicResource TextBoxBaseControlTemplate}" AllowDrop="True" IsEnabled="True" av:Canvas.Left="6" av:Canvas.Top="26"><BSkyTextBox.TextChangedBehaviour><BehaviourCollection Capacity="4"><Behaviour><Behaviour.Condition><Condition PropertyName="Text" Value="{x:Null}" Operator="isNullOrEmpty" /></Behaviour.Condition><Behaviour.Setters><PropertySettersCollection Capacity="4"><PropertySetter ControlName="m" PropertyName="CanExecute" Value="false" /></PropertySettersCollection></Behaviour.Setters></Behaviour><Behaviour><Behaviour.Condition><Condition PropertyName="Text" Value="{x:Null}" Operator="IsNumeric" /></Behaviour.Condition><Behaviour.Setters><PropertySettersCollection Capacity="4"><PropertySetter ControlName="m" PropertyName="CanExecute" Value="true" /></PropertySettersCollection></Behaviour.Setters></Behaviour><Behaviour><Behaviour.Condition><Condition PropertyName="Text" Value="{x:Null}" Operator="NotNumeric" /></Behaviour.Condition><Behaviour.Setters><PropertySettersCollection Capacity="4"><PropertySetter ControlName="m" PropertyName="CanExecute" Value="false" /></PropertySettersCollection></Behaviour.Setters></Behaviour></BehaviourCollection></BSkyTextBox.TextChangedBehaviour><BSkyTextBox.Style><av:Style TargetType="av:TextBox"><av:Style.Resources><av:ResourceDictionary /></av:Style.Resources><av:Setter Property="av:UIElement.Effect"><av:Setter.Value><av:DropShadowEffect Color="#FFF5F5DC" /></av:Setter.Value></av:Setter></av:Style></BSkyTextBox.Style>1</BSkyTextBox><BSkyLabel Name="stdlbl" Text="n (number of black balls in the urn)" Width="203" Height="27" Left="2" Top="62" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="2" av:Canvas.Top="62">n (number of black balls in the urn)</BSkyLabel><BSkyTextBox PrefixTxt="{x:Null}" OverWriteExistingVariables="False" OverwriteSettings="DontPrompt" Name="n" Width="135" Height="30" Left="6" Top="87" SubstituteSettings="TextAsIs" CanExecute="True" Enabled="True" Syntax="%%VALUE%%" TextWrapping="Wrap" AcceptsTab="True" VerticalScrollBarVisibility="Auto" FontFamily="Segoe UI" FontSize="12" VerticalContentAlignment="Center" Template="{av:DynamicResource TextBoxBaseControlTemplate}" AllowDrop="True" IsEnabled="True" av:Canvas.Left="6" av:Canvas.Top="87"><BSkyTextBox.TextChangedBehaviour><BehaviourCollection Capacity="4"><Behaviour><Behaviour.Condition><Condition PropertyName="Text" Value="{x:Null}" Operator="isNullOrEmpty" 