<BSkyCanvas internalhelpfilename="{x:Null}" Title="Uniform Distribution Plot" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="{x:Null}" CanExecute="True" PrereqCommandString="" CommandString="local(&#xA;{&#xA;#Generating the sequence of length 1000, the lower and upper bounds of the sequence are computed using the quantile function of the Uniform distribution  (qunif)  with p=0.0005 and p=0.9995 respectively &#xD;&#xA;  lowProbBound =0.0005&#xD;&#xA;  upperProbBound=0.9995&#xD;&#xA;  lowerbound =qunif(p=lowProbBound, min={{min}}, max={{max}})&#xD;&#xA;  upperbound =qunif(p=upperProbBound, min={{min}}, max={{max}})&#xD;&#xA;  .x &lt;- seq(lowerbound, upperbound, length.out=1000)  &#xA;regionslist=NULL&#xA;if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')) &amp;&amp; !is.na(as.numeric('{{reg2frm}}')) &amp;&amp; !is.na(as.numeric('{{reg2to}}')) )&#xA;{&#xA;    regionslist = list(c({{reg1frm}}, {{reg1to}}), c({{reg2frm}}, {{reg2to}}) )&#xA;}&#xA;else if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')))&#xA;{&#xA;        regionslist = list(c({{reg1frm}}, {{reg1to}}))&#xA;}&#xA;&#xA;if({{plotdenfun}} {{plotdistfun}})&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::dunif(.x, min={{min}}, max={{max}}), cdf=FALSE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Density&quot;, &#xA;  main=paste(&quot;Uniform Distribution:  Minumum={{min}}, Maximum={{max}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;else&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::punif(.x, min={{min}}, max={{max}}), cdf=TRUE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Cumulative Probability&quot;, &#xA;  main=paste(&quot;Uniform Distribution:  Minumum={{min}}, Maximum={{max}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})}&#xA;&#xA;}&#xA;)" dlgsyntax="local(&#xA;{&#xA;#Generating the sequence of length 1000, the lower and upper bounds of the sequence are computed using the quantile function of the Uniform distribution  (qunif)  with p=0.0005 and p=0.9995 respectively &#xD;&#xA;  lowProbBound =0.0005&#xD;&#xA;  upperProbBound=0.9995&#xD;&#xA;  lowerbound =qunif(p=lowProbBound, min={{min}}, max={{max}})&#xD;&#xA;  upperbound =qunif(p=upperProbBound, min={{min}}, max={{max}})&#xD;&#xA;  .x &lt;- seq(lowerbound, upperbound, length.out=1000)  &#xA;regionslist=NULL&#xA;if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')) &amp;&amp; !is.na(as.numeric('{{reg2frm}}')) &amp;&amp; !is.na(as.numeric('{{reg2to}}')) )&#xA;{&#xA;    regionslist = list(c({{reg1frm}}, {{reg1to}}), c({{reg2frm}}, {{reg2to}}) )&#xA;}&#xA;else if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')))&#xA;{&#xA;        regionslist = list(c({{reg1frm}}, {{reg1to}}))&#xA;}&#xA;&#xA;if({{plotdenfun}} {{plotdistfun}})&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::dunif(.x, min={{min}}, max={{max}}), cdf=FALSE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Density&quot;, &#xA;  main=paste(&quot;Uniform Distribution:  Minumum={{min}}, Maximum={{max}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;else&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::punif(.x, min={{min}}, max={{max}}), cdf=TRUE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Cumulative Probability&quot;, &#xA;  main=paste(&quot;Uniform Distribution:  Minumum={{min}}, Maximum={{max}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})}&#xA;&#xA;}&#xA;)" Helpfile="{x:Null}" splitProcessing="True" DatasetRequired="False" MenuLocation="{x:Null}" OutputDefinition="" Width="370" Command="False" Height="455" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold } .c2 { margin: 0px 0px 10px } .c3 { font-family: 'Courier New' } .c4 { vertical-align: middle } .c5 { vertical-align: top } .c6 { margin: 0px 0px 12px } .c7 { font-style: italic } .c8 { margin: 0px 0px 10px; text-align: center } .c9 { color: Blue; text-decoration: underline; font-family: 'Courier New' } .c10 { font-weight: bold; font-family: 'Courier New','Courier'; color: #666666 } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c1&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;span class=&quot;c1&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c2&quot;&gt;These functions provide information about the uniform distribution on the interval from&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;min&lt;/span&gt;&lt;/span&gt;&amp;#160;to&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;max&lt;/span&gt;&lt;/span&gt;.&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dunif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the density,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;punif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the distribution function&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qunif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the quantile function and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;runif&lt;/span&gt;&lt;/span&gt;&amp;#160;generates random deviates.&lt;/p&gt;&lt;p class=&quot;c2&quot;&gt;#Generating the sequence of length 1000, the lower and upper bounds of the sequence are computed using the quantile function of the Uniform distribution &amp;#160;(qunif) &amp;#160;with p=0.0005 and p=0.9995 respectively&amp;#160;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c1&quot;&gt;Usage&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dunif(x, min = 0, max = 1, log = FALSE)&lt;br/&gt;punif(q, min = 0, max = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;qunif(p, min = 0, max = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;runif(n, min = 0, max = 1)&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c1&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c4&quot;&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;x, q&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of quantiles.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of probabilities.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;number of observations. If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;length(n) &amp;gt; 1&lt;/span&gt;&lt;/span&gt;, the length is taken to be the number required.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;min, max&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;lower and upper limits of the distribution. Must be finite.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;log, log.p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE, probabilities p are given as log(p).&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;lower.tail&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE (default), probabilities are&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X ≤ x]&lt;/span&gt;&lt;/span&gt;, otherwise,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X &amp;gt; x]&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c1&quot;&gt;Details&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c2&quot;&gt;If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;min&lt;/span&gt;&lt;/span&gt;&amp;#160;or&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;max&lt;/span&gt;&lt;/span&gt;&amp;#160;are not specified they assume the default values of&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;0&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;1&lt;/span&gt;&lt;/span&gt;&amp;#160;respectively.&lt;/p&gt;&lt;p class=&quot;c2&quot;&gt;The uniform distribution has density&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;f(x) = 1/(max-min)&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c2&quot;&gt;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;min ≤ x ≤ max&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;p class=&quot;c2&quot;&gt;For the case of&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;u := min == max&lt;/span&gt;&lt;/span&gt;, the limit case of&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;X == u&lt;/span&gt;&lt;/span&gt;&amp;#160;is assumed, although there is no density in that case and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dunif&lt;/span&gt;&lt;/span&gt;&amp;#160;will return&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;NaN&lt;/span&gt;&lt;/span&gt;&amp;#160;(the error condition).&lt;/p&gt;&lt;p class=&quot;c2&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;runif&lt;/span&gt;&lt;/span&gt;&amp;#160;will not generate either of the extreme values unless&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;max = min&lt;/span&gt;&lt;/span&gt;&amp;#160;or&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;max-min&lt;/span&gt;&lt;/span&gt;&amp;#160;is small compared to&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;min&lt;/span&gt;&lt;/span&gt;, and in particular not for the default arguments.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c1&quot;&gt;Value&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c2&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dunif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the density,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;punif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the distribution function,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qunif&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the quantile function, and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;runif&lt;/span&gt;&lt;/span&gt;&amp;#160;generates random deviates.&lt;/p&gt;&lt;p class=&quot;c2&quot;&gt;The length of the result is determined by&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;runif&lt;/span&gt;&lt;/span&gt;, and is the maximum of the lengths of the numerical arguments for the other functions.&lt;/p&gt;&lt;p class=&quot;c2&quot;&gt;The numerical arguments other than&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;are recycled to the length of the result. Only the first elements of the logical arguments are used.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c1&quot;&gt;Note&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c2&quot;&gt;The characteristics of output from pseudo-random number generators (such as precision and periodicity) vary widely. See&amp;#160;&lt;a href=&quot;http://127.0.0.1:21039/library/stats/help/.Random.seed&quot; class=&quot;c9&quot;&gt;.Random.seed&lt;/a&gt;&amp;#160;for more information on&amp;#160;&lt;span&gt;&lt;span class=&quot;c10&quot;&gt;R&lt;/span&gt;&lt;/span&gt;'s random number generation algorithms.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(dunif , package=stats)" Background="#FFFFFFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"><BSkyLabel Name="meanlbl" Text="Minimum" Width="63" Height="24" Left="2" Top="3" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="2" av:Canvas.Top="3">Minimum</BSkyLabel><BSkyLabel Name="stddevi" Text="Maximum" Width="66" Height="27" Left="2" Top="70" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="2" av:Canvas.Top="70">Maximum</BSkyLabel><BSkyRadioButton GroupName="a" Name="plotdenfun" Text="Plot density function" Width="137" Left="0" Top="0" Height="22" IsSelected="True" CanExecute="True" Enabled="True" Syntax="TRUE" IsChecked="True" FontFamily="Segoe UI" FontSize="12" Style="{av:DynamicResource RadioButtonTemplate}" IsEnabled="True" av:Canvas.Left="6" av:Canvas.Top="147"><BSkyRadioButton.CheckedChangeBehaviour><BehaviourCollection Capacity="0" /></BSkyRadioButton.CheckedChangeBehaviour>Plot density function</BSkyRadioButton><BSkyRadioButton GroupName="a" Name="plotdistfun" Text="Plot distribution function" Width="171" Left="0" Top="0" Height="25" IsSelected="False" CanExecute="True" Enabled="True" Syntax="FALSE" IsChecked="False" FontFamily="Segoe UI" FontSize="12" Style="{av:DynamicResource RadioButtonTemplate}" IsEnabled="True" av:Canvas.Left="6" av:Canvas.Top="166"><BSkyRadioButton.CheckedChangeBehaviour><BehaviourCollection Capacity="0" /></BSkyRadioButton.CheckedChangeBehaviour>Plot distribution function</BSkyRadioButton><BSkyLabel Name="lbl1" Text="Optionally specify regions under the density funtion by" Width="327" Height="26" Left="1" Top="195" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas