<BSkyCanvas internalhelpfilename="{x:Null}" Title="Beta Distribution Plot" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="{x:Null}" CanExecute="True" PrereqCommandString="" CommandString="local(&#xA;{&#xA;#Generating the sequence of length 1000, the lower and upper bounds of the sequence are computed using the quantile function of the beta distribution  (qbeta)  with p=0.0005 and p=0.9995 respectively &#xA;lowProbBound =0.0005&#xA;upperProbBound=0.9995&#xA;lowerbound =qbeta(p=lowProbBound, shape1={{shape1}}, shape2={{shape2}})&#xA;upperbound =qbeta(p=upperProbBound, shape1={{shape1}}, shape2={{shape2}})&#xA;#cat(paste(&quot;Plotting a beta distribution using a sequence of length 1000, with lower bound &quot;,lowerbound,&quot; an upper bound &quot;,upperbound,&quot;. \nThe lower and upper bounds of the sequence are computed using the quantile function of the beta distribution (qbeta) with probability p=0.0005 and p=0.9995 respectively&quot;, collapse=&quot;&quot;,sep=&quot;&quot; ))&#xA;  .x &lt;- seq(lowerbound, upperbound, length.out=1000)  &#xA;&#xA;#Optionally capturing regions to fill&#xA;regionslist=NULL&#xA;if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')) &amp;&amp; !is.na(as.numeric('{{reg2frm}}')) &amp;&amp; !is.na(as.numeric('{{reg2to}}')) )&#xA;{&#xA;    regionslist = list(c({{reg1frm}}, {{reg1to}}), c({{reg2frm}}, {{reg2to}}) )&#xA;}&#xA;else if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')))&#xA;{&#xA;        regionslist = list(c({{reg1frm}}, {{reg1to}}))&#xA;}&#xA;&#xA;#Plot a density function&#xA;if({{plotdenfun}}{{plotdistfun}})&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::dbeta(.x, shape1={{shape1}}, shape2={{shape2}}), cdf=FALSE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Density&quot;, &#xA;  main=paste(&quot;Beta Distribution:  Shape 1={{shape1}}, Shape 2={{shape2}}&quot;),&#xA;regions=regionslist , col=c('{{col1}}', '{{col2}}'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;#Plot a distribution function&#xA;else&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::pbeta(.x, shape1={{shape1}}, shape2={{shape2}}), cdf=TRUE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Cumulative Probability&quot;, &#xA;  main=paste(&quot;Beta Distribution:  Shape 1={{shape1}}, Shape 2={{shape2}}&quot;),&#xA;regions=regionslist , col=c('{{col1}}', '{{col2}}'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;}&#xA;)" dlgsyntax="local(&#xA;{&#xA;#Generating the sequence of length 1000, the lower and upper bounds of the sequence are computed using the quantile function of the beta distribution  (qbeta)  with p=0.0005 and p=0.9995 respectively &#xA;lowProbBound =0.0005&#xA;upperProbBound=0.9995&#xA;lowerbound =qbeta(p=lowProbBound, shape1={{shape1}}, shape2={{shape2}})&#xA;upperbound =qbeta(p=upperProbBound, shape1={{shape1}}, shape2={{shape2}})&#xA;#cat(paste(&quot;Plotting a beta distribution using a sequence of length 1000, with lower bound &quot;,lowerbound,&quot; an upper bound &quot;,upperbound,&quot;. \nThe lower and upper bounds of the sequence are computed using the quantile function of the beta distribution (qbeta) with probability p=0.0005 and p=0.9995 respectively&quot;, collapse=&quot;&quot;,sep=&quot;&quot; ))&#xA;  .x &lt;- seq(lowerbound, upperbound, length.out=1000)  &#xA;&#xA;#Optionally capturing regions to fill&#xA;regionslist=NULL&#xA;if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')) &amp;&amp; !is.na(as.numeric('{{reg2frm}}')) &amp;&amp; !is.na(as.numeric('{{reg2to}}')) )&#xA;{&#xA;    regionslist = list(c({{reg1frm}}, {{reg1to}}), c({{reg2frm}}, {{reg2to}}) )&#xA;}&#xA;else if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')))&#xA;{&#xA;        regionslist = list(c({{reg1frm}}, {{reg1to}}))&#xA;}&#xA;&#xA;#Plot a density function&#xA;if({{plotdenfun}}{{plotdistfun}})&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::dbeta(.x, shape1={{shape1}}, shape2={{shape2}}), cdf=FALSE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Density&quot;, &#xA;  main=paste(&quot;Beta Distribution:  Shape 1={{shape1}}, Shape 2={{shape2}}&quot;),&#xA;regions=regionslist , col=c('{{col1}}', '{{col2}}'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;#Plot a distribution function&#xA;else&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::pbeta(.x, shape1={{shape1}}, shape2={{shape2}}), cdf=TRUE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Cumulative Probability&quot;, &#xA;  main=paste(&quot;Beta Distribution:  Shape 1={{shape1}}, Shape 2={{shape2}}&quot;),&#xA;regions=regionslist , col=c('{{col1}}', '{{col2}}'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;}&#xA;)" Helpfile="{x:Null}" splitProcessing="True" DatasetRequired="False" MenuLocation="{x:Null}" OutputDefinition="" Width="370" Command="False" Height="455" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold } .c3 { font-family: 'Courier New' } .c4 { vertical-align: middle } .c5 { vertical-align: top } .c6 { margin: 0px 0px 12px } .c7 { font-style: italic } .c8 { margin: 0px 0px 10px; text-align: center } .c9 { color: Blue; text-decoration: underline; font-family: 'Courier New' } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;Density, distribution function, quantile function and random generation for the Beta distribution with parameters&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;shape1&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;shape2&lt;/span&gt;&lt;/span&gt;&amp;#160;(and optional non-centrality parameter&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;ncp&lt;/span&gt;&lt;/span&gt;).&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;#Generating the sequence of length 1000, the lower and upper bounds of the sequence are computed using the quantile function of the beta distribution &amp;#160;(qbeta) &amp;#160;with p=0.0005 and p=0.9995 respectively&amp;#160;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Usage&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dbeta(x, shape1, shape2, ncp = 0, log = FALSE)&lt;br/&gt;pbeta(q, shape1, shape2, ncp = 0, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;qbeta(p, shape1, shape2, ncp = 0, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;rbeta(n, shape1, shape2, ncp = 0)&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c4&quot;&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;x, q&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of quantiles.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of probabilities.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;number of observations. If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;length(n) &amp;gt; 1&lt;/span&gt;&lt;/span&gt;, the length is taken to be the number required.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;shape1, shape2&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;non-negative parameters of the Beta distribution.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;ncp&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;non-centrality parameter.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;log, log.p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE, probabilities p are given as log(p).&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;lower.tail&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE (default), probabilities are&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X ≤ x]&lt;/span&gt;&lt;/span&gt;, otherwise,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X &amp;gt; x]&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Details&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;The Beta distribution with parameters&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;shape1&lt;/span&gt;&lt;/span&gt;&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;= a&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;shape2&lt;/span&gt;&lt;/span&gt;&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;= b&lt;/span&gt;&lt;/span&gt;&amp;#160;has density&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;Γ(a+b)/(Γ(a)Γ(b))x^(a-1)(1-x)^(b-1)&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;a &amp;gt; 0&lt;/span&gt;&lt;/span&gt;,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;b &amp;gt; 0&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;0 ≤ x ≤ 1&lt;/span&gt;&lt;/span&gt;&amp;#160;where the boundary values at&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;x=0&lt;/span&gt;&lt;/span&gt;&amp;#160;or&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;x=1&lt;/span&gt;&lt;/span&gt;&amp;#160;are defined as by continuity (as limits). &lt;br/&gt; The mean is&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;a/(a+b)&lt;/span&gt;&lt;/span&gt;&amp;#160;and the variance is&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;ab/((a+b)^2 (a+b+1))&lt;/span&gt;&lt;/span&gt;. These moments and all distributional properties can be defined as limits (leading to point masses at 0, 1/2, or 1) when&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;a&lt;/span&gt;&lt;/span&gt;&amp;#160;or&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;b&lt;/span&gt;&lt;/span&gt;&amp;#160;are zero or infinite, and the corresponding&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;[dpqr]beta()&lt;/span&gt;&lt;/span&gt;&amp;#160;functions are defined correspondingly.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;pbeta&lt;/span&gt;&lt;/span&gt;&amp;#160;is closely related to the incomplete beta function. As defined by Abramowitz and Stegun 6.6.1&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;B_x(a,b) = integral_0^x t^(a-1) (1-t)^(b-1) dt,&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;and 6.6.2&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;I_x(a,b) = B_x(a,b) / B(a,b)&lt;/span&gt;&lt;/span&gt;&amp;#160;where&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;B(a,b) = B_1(a,b)&lt;/span&gt;&lt;/span&gt;&amp;#160;is the Beta function (&lt;a href=&quot;http://127.0.0.1:21039/library/stats/help/beta&quot; class=&quot;c9&quot;&gt;beta&lt;/a&gt;).&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;I_x(a,b)&lt;/span&gt;&lt;/span&gt;&amp;#160;is&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;pbeta(x, a, b)&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The noncentral Beta distribution (with&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;ncp&lt;/span&gt;&lt;/span&gt;&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;= λ&lt;/span&gt;&lt;/span&gt;) is defined (Johnson&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;et al&lt;/span&gt;&lt;/span&gt;, 1995, pp. 502) as the distribution of&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;X/(X+Y)&lt;/span&gt;&lt;/span&gt;&amp;#160;where&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;X ~ chi^2_2a(λ)&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;Y ~ chi^2_2b&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Value&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dbeta&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the density,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;pbeta&lt;/span&gt;&lt;/span&gt;&amp;#160;the distribution function,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qbeta&lt;/span&gt;&lt;/span&gt;&amp;#160;the quantile function, and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rbeta&lt;/span&gt;&lt;/span&gt;&amp;#160;generates random deviates.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;Invalid arguments will result in return value&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;NaN&lt;/span&gt;&lt;/span&gt;, with a warning.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The length of the result is determined by&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rbeta&lt;/span&gt;&lt;/span&gt;, and is the maximum of the lengths of the numerical arguments for the other functions.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The numerical arguments other than&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;are recycled to the length of the result. Only the first elements of the logical arguments are used.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Note&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;Supplying&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;ncp = 0&lt;/span&gt;&lt;/span&gt;&amp;#160;uses the algorithm for the non-central distribution, which is not the same algorithm used if&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;ncp&lt;/span&gt;&lt;/span&gt;&amp;#160;is omitted. This is to give consistent behaviour in extreme cases with values of&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;ncp&lt;/span&gt;&lt;/span&gt;&amp;#160;very near zero.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(dbeta , package=stats)" Background="#FFFFFFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"><BSkyLabel Name="meanlbl" Text="Shape 1" Width="54" Height="26" Left="2" Top="3" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="2" av:Canvas.Top="3">Shape 1</BSkyLabel><BSkyLabel Name="stddevi" Text="Shape 2" Width="56" Height="26" Left="2" Top="70" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="2" av:Canvas.Top="70">Shape 2</BSkyLabel><BSkyRadioButton GroupName="a" Name="plotdenfun" Text="Plot density function" Width="137" Left="0" Top="0" Height="22" IsSelected="True" CanExecute="True" Enabled="True" Syntax="TRUE" IsChecked="True" FontFamily="Segoe UI" FontSize="12" Style="{av:DynamicResource RadioButtonTemplate}" IsEnabled="True" av: