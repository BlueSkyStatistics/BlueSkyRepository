<BSkyCanvas internalhelpfilename="{x:Null}" Title="Confidence Interval" ModelClasses="glm; lm; polr; multinom" StatusTextBoxName="msgbox" RPackages="MASS; RcmdrMisc" CanExecute="True" PrereqCommandString="StepwisePrerequisite('{{%MODEL%}}', '{{%DATASET%}}', NAinVarsCheck=FALSE )" CommandString="local(&#xA;{&#xA;tmpbsky &lt;- BlueSky::confint({{%MODEL%}}, level={{conlevel}}, type=&quot;{{lr}}{{wald}}&quot;)&#xA;BSkyFormat(tmpbsky, singleTableOutputHeader =&quot;Confidence Interval&quot;) &#xA;}&#xA;)&#xA;" dlgsyntax="local(&#xA;{&#xA;tmpbsky &lt;- BlueSky::confint({{%MODEL%}}, level={{conlevel}}, type=&quot;{{lr}}{{wald}}&quot;)&#xA;BSkyFormat(tmpbsky, singleTableOutputHeader =&quot;Confidence Interval&quot;) &#xA;}&#xA;)&#xA;" Helpfile="{x:Null}" splitProcessing="True" MenuLocation="{x:Null}" OutputDefinition="{x:Null}" Width="300" Command="False" Height="250" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { margin: 0px 0px 11px; font-size: 12px; font-weight: bold } .c3 { font-size: 12px } .c4 { font-family: 'Courier New'; font-size: 12px } .c5 { color: Blue; text-decoration: underline; font-family: 'Courier New' } .c6 { vertical-align: middle } .c7 { vertical-align: top } .c8 { margin: 0px 0px 12px } .c9 { font-weight: bold; font-size: 12px } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c3&quot;&gt;Except for&amp;#160;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;&amp;quot;glm&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;&amp;quot;multinom&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c3&quot;&gt;, and&amp;#160;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;&amp;quot;polr&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;objects, this generic function simply calls&amp;#160;&lt;/span&gt;&lt;a href=&quot;http://127.0.0.1:15433/library/stats/html/confint.html&quot; class=&quot;c5&quot;&gt;&lt;span class=&quot;c3&quot;&gt;confint&lt;/span&gt;&lt;/a&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;in the&amp;#160;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;stats&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;package which computes confidence intervals for one or more parameters in a fitted model.&amp;#160;&lt;/span&gt;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Usage&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;Confint(object, parm, level = 0.95, ...)&lt;br/&gt;&lt;br/&gt;## S3 method for class 'glm'&lt;br/&gt;Confint(object, parm, level=0.95, type=c(&amp;quot;LR&amp;quot;, &amp;quot;Wald&amp;quot;), ...)&lt;br/&gt;## S3 method for class 'multinom'&lt;br/&gt;Confint(object, parm, level = 0.95, ...)&lt;br/&gt;## S3 method for class 'polr'&lt;br/&gt;Confint(object, parm, level=0.95, ...)&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c6&quot;&gt;&lt;tr class=&quot;c7&quot;&gt;&lt;td class=&quot;c7&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;objec&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;t&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c7&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#x200b;&lt;/span&