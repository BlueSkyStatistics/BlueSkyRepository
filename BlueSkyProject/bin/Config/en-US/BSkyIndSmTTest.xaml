<BSkyCanvas internalhelpfilename="{x:Null}" Title="Multi Variable Independent Sample T-Test" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="{x:Null}" CanExecute="True" PrereqCommandString="{x:Null}" CommandString="# Run the T-test&#xD;&#xA;BSky_Indpendent_Sample_T_Test = BSkyIndSmTTest(varNamesOrVarGlobalIndices=c({{Target}}), group=c('{{Target2}}'),conf.level={{confidenceInterval}},alternative='{{gpbox2}}',missing ={{missingVals}},datasetNameOrDatasetGlobalIndex='{{%DATASET%}}') &#xA;# Display the results&#xA;BSkyFormat(BSky_Indpendent_Sample_T_Test)&#xA;#remove(BSky_Indpendent_Sample_T_Test)" dlgsyntax="# Run the T-test&#xD;&#xA;BSky_Indpendent_Sample_T_Test = BSkyIndSmTTest(varNamesOrVarGlobalIndices=c({{Target}}), group=c('{{Target2}}'),conf.level={{confidenceInterval}},alternative='{{gpbox2}}',missing ={{missingVals}},datasetNameOrDatasetGlobalIndex='{{%DATASET%}}') &#xA;# Display the results&#xA;BSkyFormat(BSky_Indpendent_Sample_T_Test)&#xA;#remove(BSky_Indpendent_Sample_T_Test)" Helpfile="{x:Null}" splitProcessing="True" MenuLocation="{x:Null}" OutputDefinition="C:\Users\aaron.rangel\Documents\Predictive Analytics\Delete\Commercial\Means\templated\BSkyIndSmTTest.xml" Width="490" Command="False" Height="400" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { font-size: 12px; font-weight: bold } .c3 { font-size: 12px } .c4 { font-size: 12px; font-family: 'Arial' } .c5 { background-color: Transparent; color: Black; font-family: 'Arial'; font-size: 12px; font-style: normal; font-weight: normal; text-align: left; text-decoration: none; text-indent: 0px } .c6 { border-spacing: 0px; width: 643px } .c7 { width: 230px } .c8 { width: 397px } .c9 { vertical-align: middle } .c10 { vertical-align: top } .c11 { padding: 0px; vertical-align: top; width: 230px } .c12 { margin: 0px 0px 9px; line-height: 1.2 } .c13 { padding: 0px; vertical-align: top; width: 397px } .c14 { font-weight: bold; font-size: 13.3333333333333px } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Performs a one sample t-tests against the two groups formed by the factor variable (with two levels).&amp;#160;&lt;/span&gt;&lt;span class=&quot;c4&quot;&gt;Displays results for equal &amp;#160;&amp;#160;variances TRUE and FALSE. If equal variances&amp;#160;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;then the pooled variance is used to estimate the variance otherwise the Welch (or Satterthwaite) approximation to the degrees of freedom is used.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Usage&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;BSkyIndSmTTest(varNamesOrVarGlobalIndices=c('var1','var2'), group=c('var3'), conf.level = 0.95, alternative=&amp;quot;two.sided&amp;quot;, missing =0, datasetNameOrDatasetGlobalIndex = 'Dataset')&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;table class=&quot;c6&quot;&gt;&lt;col class=&quot;c7&quot;/&gt;&lt;col class=&quot;c8&quot;/&gt;&lt;tbody class=&quot;c9&quot;&gt;&lt;tr class=&quot;c10&quot;&gt;&lt;td class=&quot;c11&quot;&gt;&lt;p class=&quot;c12&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;varNamesOrVarGlobalIndices&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c12&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;selected scale variables (say var1, var2)&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c10&quot;&gt;&lt;td class=&quot;c11&quot;&gt;&lt;p class=&quot;c12&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;group&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c12&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;a factor variable with two levels (say var3)&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c10&quot;&gt;&lt;td class=&quot;c11&quot;&gt;&lt;p class=&quot;c12&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;conf.level&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c12&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;a numeric value &amp;#160;(say 0.95) .&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c10&quot;&gt;&lt;td class=&quot;c11&quot;&gt;&lt;p class=&quot;c12&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;missing&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c12&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;missing values are handled on a per variable basis (missing =0) or list wise across all variables (missing=1).&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c10&quot;&gt;&lt;td class=&quot;c11&quot;&gt;&lt;p class=&quot;c12&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;datasetNameOrDatasetGlobalIndex&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c12&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Name of the dataset (say Dataset) &amp;#160;from which var1, var2 and var3 are selected.&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;alternative: &amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;a character string specifying the alternative hypothesis, must be one of&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;&amp;quot;two.sided&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;&amp;#160;&amp;#160;(default),&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;&amp;quot;greater&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;&amp;#160;or&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;&amp;quot;less&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;. You can specify just the initial letter.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Value&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;A list with resulting tables is returned.&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Example&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Dataset &amp;lt;- data.frame(Expenses=c(20,23,19,25,26), Sales=c(48,50,55,51,49), Gender=c('m','f','f','m','m'), Deptt=c('IT', 'Sales', 'IT','Sales','IT'))&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;BSkyLoadRefreshDataframe(Dataset) # Dataset must be loaded in the UI grid&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;BSky_One_Simple_T_Test = BSkyIndSmTTest(varNamesOrVarGlobalIndices =c('Sales','Expenses'),group=c('Deptt'),conf.level=0.95, alternative=&amp;quot;less&amp;quot;, datasetNameOrDatasetGlobalIndex ='Dataset