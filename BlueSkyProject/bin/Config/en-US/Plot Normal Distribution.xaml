<BSkyCanvas internalhelpfilename="{x:Null}" Title="Normal Distribution Plot" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="{x:Null}" CanExecute="True" PrereqCommandString="" CommandString="local(&#xA;{&#xA;  .x &lt;- seq(-3.291, 3.291, length.out=1000)  &#xA;regionslist=NULL&#xA;if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')) &amp;&amp; !is.na(as.numeric('{{reg2frm}}')) &amp;&amp; !is.na(as.numeric('{{reg2to}}')) )&#xA;{&#xA;    regionslist = list(c({{reg1frm}}, {{reg1to}}), c({{reg2frm}}, {{reg2to}}) )&#xA;}&#xA;else if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')))&#xA;{&#xA;        regionslist = list(c({{reg1frm}}, {{reg1to}}))&#xA;}&#xA;&#xA;if({{plotdenfun}} {{plotdistfun}})&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::dnorm(.x, mean={{mean}}, sd={{stddev}}), cdf=FALSE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Density&quot;, &#xA;  main=paste(&quot;Normal Distribution:  Mean={{mean}}, Standard deviation={{stddev}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;else&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::pnorm(.x, mean={{mean}}, sd={{stddev}}), cdf=TRUE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Cumulative Probability&quot;, &#xA;  main=paste(&quot;Normal Distribution:  Mean={{mean}}, Standard deviation={{stddev}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;&#xA;}&#xA;)" dlgsyntax="local(&#xA;{&#xA;  .x &lt;- seq(-3.291, 3.291, length.out=1000)  &#xA;regionslist=NULL&#xA;if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')) &amp;&amp; !is.na(as.numeric('{{reg2frm}}')) &amp;&amp; !is.na(as.numeric('{{reg2to}}')) )&#xA;{&#xA;    regionslist = list(c({{reg1frm}}, {{reg1to}}), c({{reg2frm}}, {{reg2to}}) )&#xA;}&#xA;else if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')))&#xA;{&#xA;        regionslist = list(c({{reg1frm}}, {{reg1to}}))&#xA;}&#xA;&#xA;if({{plotdenfun}} {{plotdistfun}})&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::dnorm(.x, mean={{mean}}, sd={{stddev}}), cdf=FALSE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Density&quot;, &#xA;  main=paste(&quot;Normal Distribution:  Mean={{mean}}, Standard deviation={{stddev}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;else&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::pnorm(.x, mean={{mean}}, sd={{stddev}}), cdf=TRUE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Cumulative Probability&quot;, &#xA;  main=paste(&quot;Normal Distribution:  Mean={{mean}}, Standard deviation={{stddev}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;&#xA;}&#xA;)" Helpfile="{x:Null}" splitProcessing="True" DatasetRequired="False" MenuLocation="{x:Null}" OutputDefinition="" Width="370" Command="False" Height="455" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold } .c3 { font-family: 'Courier New' } .c4 { vertical-align: middle } .c5 { vertical-align: top } .c6 { margin: 0px 0px 12px } .c7 { font-style: italic } .c8 { margin: 0px 0px 10px; text-align: center } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&amp;#x200b;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;Density, distribution function, quantile function and random generation for the normal distribution with mean equal to&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;mean&lt;/span&gt;&lt;/span&gt;&amp;#160;and standard deviation equal to&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;sd&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Usage&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dnorm(x, mean = 0, sd = 1, log = FALSE)&lt;br/&gt;pnorm(q, mean = 0, sd = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;qnorm(p, mean = 0, sd = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;rnorm(n, mean = 0, sd = 1)&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c4&quot;&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;x, q&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of quantiles.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of probabilities.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;number of observations. If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;length(n) &amp;gt; 1&lt;/span&gt;&lt;/span&gt;, the length is taken to be the number required.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;mean&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of means.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;sd&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of standard deviations.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;log, log.p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE, probabilities p are given as log(p).&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;lower.tail&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE (default), probabilities are&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X ≤ x]&lt;/span&gt;&lt;/span&gt;&amp;#160;otherwise,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X &amp;gt; x]&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Details&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;mean&lt;/span&gt;&lt;/span&gt;&amp;#160;or&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;sd&lt;/span&gt;&lt;/span&gt;&amp;#160;are not specified they assume the default values of&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;0&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;1&lt;/span&gt;&lt;/span&gt;, respectively.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The normal distribution has density&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;f(x) = 1/(√(2 π) σ) e^-((x - μ)^2/(2 σ^2))&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;where&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;μ&lt;/span&gt;&lt;/span&gt;&amp;#160;is the mean of the distribution and&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;σ&lt;/span&gt;&lt;/span&gt;&amp;#160;the standard deviation.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Value&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the density,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;pnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the distribution function,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the quantile function, and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;generates random deviates.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The length of the result is determined by&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rnorm&lt;/span&gt;&lt;/span&gt;, and is the maximum of the lengths of the numerical arguments for the other functions.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The numerical arguments other than&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;are recycled to the length of the result. Only the first elements of the logical arguments are used.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;For&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;sd = 0&lt;/span&gt;&lt;/span&gt;&amp;#160;this gives the limit as&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;sd&lt;/span&gt;&lt;/span&gt;&amp;#160;decreases to 0, a point mass at&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;mu&lt;/span&gt;&lt;/span&gt;.&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;sd &amp;lt; 0&lt;/span&gt;&lt;/span&gt;&amp;#160;is an error and returns&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;NaN&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(dnorm , package=stats)" Background="#FFFFFFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns: