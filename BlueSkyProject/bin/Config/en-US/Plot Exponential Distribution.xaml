<BSkyCanvas internalhelpfilename="{x:Null}" Title="Exponential Distribution Plot" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="{x:Null}" CanExecute="True" PrereqCommandString="" CommandString="local(&#xA;{&#xA;  .x &lt;- seq(0.001, 7.601, length.out=1000)  &#xA;regionslist=NULL&#xA;if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')) &amp;&amp; !is.na(as.numeric('{{reg2frm}}')) &amp;&amp; !is.na(as.numeric('{{reg2to}}')) )&#xA;{&#xA;    regionslist = list(c({{reg1frm}}, {{reg1to}}), c({{reg2frm}}, {{reg2to}}) )&#xA;}&#xA;else if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')))&#xA;{&#xA;        regionslist = list(c({{reg1frm}}, {{reg1to}}))&#xA;}&#xA;&#xA;if({{plotdenfun}} {{plotdistfun}})&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::dexp(.x, rate={{rate}}), cdf=FALSE, &#xA;xlab=&quot;x&quot;,  ylab=&quot;Density&quot;, &#xA;  main=paste(&quot;Exponential Distribution:  Rate={{rate}}&quot;),&#xA;regions=regionslist, col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;else&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::pexp(.x, rate={{rate}}), cdf=TRUE, &#xA;xlab=&quot;x&quot;,  ylab=&quot;Cumulative Probability&quot;, &#xA;  main=paste(&quot;Exponential Distribution:  Rate={{rate}}&quot;),&#xA;regions=regionslist, col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;&#xA;}&#xA;)" dlgsyntax="local(&#xA;{&#xA;  .x &lt;- seq(0.001, 7.601, length.out=1000)  &#xA;regionslist=NULL&#xA;if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')) &amp;&amp; !is.na(as.numeric('{{reg2frm}}')) &amp;&amp; !is.na(as.numeric('{{reg2to}}')) )&#xA;{&#xA;    regionslist = list(c({{reg1frm}}, {{reg1to}}), c({{reg2frm}}, {{reg2to}}) )&#xA;}&#xA;else if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')))&#xA;{&#xA;        regionslist = list(c({{reg1frm}}, {{reg1to}}))&#xA;}&#xA;&#xA;if({{plotdenfun}} {{plotdistfun}})&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::dexp(.x, rate={{rate}}), cdf=FALSE, &#xA;xlab=&quot;x&quot;,  ylab=&quot;Density&quot;, &#xA;  main=paste(&quot;Exponential Distribution:  Rate={{rate}}&quot;),&#xA;regions=regionslist, col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;else&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::pexp(.x, rate={{rate}}), cdf=TRUE, &#xA;xlab=&quot;x&quot;,  ylab=&quot;Cumulative Probability&quot;, &#xA;  main=paste(&quot;Exponential Distribution:  Rate={{rate}}&quot;),&#xA;regions=regionslist, col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;&#xA;}&#xA;)" Helpfile="{x:Null}" splitProcessing="True" DatasetRequired="False" MenuLocation="{x:Null}" OutputDefinition="" Width="370" Command="False" Height="377" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold } .c3 { font-family: 'Courier New' } .c4 { vertical-align: middle } .c5 { vertical-align: top } .c6 { margin: 0px 0px 12px } .c7 { font-style: italic } .c8 { margin: 0px 0px 10px; text-align: center } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&amp;#x200b;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;Density, distribution function, quantile function and random generation for the exponential distribution with rate&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rate&lt;/span&gt;&lt;/span&gt;&amp;#160;(i.e., mean&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;1/rate&lt;/span&gt;&lt;/span&gt;).&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Usage&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dexp(x, rate = 1, log = FALSE)&lt;br/&gt;pexp(q, rate = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;qexp(p, rate = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;rexp(n, rate = 1)&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c4&quot;&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;x, q&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of quantiles.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of probabilities.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;number of observations. If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;length(n) &amp;gt; 1&lt;/span&gt;&lt;/span&gt;, the length is taken to be the number required.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rate&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of rates.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;log, log.p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE, probabilities p are given as log(p).&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;lower.tail&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE (default), probabilities are&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X ≤ x]&lt;/span&gt;&lt;/span&gt;, otherwise,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X &amp;gt; x]&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Details&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rate&lt;/span&gt;&lt;/span&gt;&amp;#160;is not specified, it assumes the default value of&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;1&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The exponential distribution with rate&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;λ&lt;/span&gt;&lt;/span&gt;&amp;#160;has density&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;f(x) = λ {e}^{- λ x}&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;x ≥ 0&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Value&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dexp&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the density,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;pexp&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the distribution function,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qexp&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the quantile function, and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rexp&lt;/span&gt;&lt;/span&gt;&amp;#160;generates random deviates.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The length of the result is determined by&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rexp&lt;/span&gt;&lt;/span&gt;, and is the maximum of the lengths of the numerical arguments for the other functions.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The numerical arguments other than&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;are recycled to the length of the result. Only the first elements of the logical arguments are used.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Note&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;The cumulative hazard&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;H(t) = - log(1 - F(t))&lt;/span&gt;&lt;/span&gt;&amp;#160;is&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;-pexp(t, r, lower = FALSE, log = TRUE)&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(dexp , package=stats)" Background="#FFFFFFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"><BSkyLabel Name="meanlbl" Text="Rate" Width="123" Height="29" Left="2" Top="3" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="2" av:Canvas.Top="3">Rate</BSkyLabel><BSkyRadioButton GroupName="a" Name="plotdenfun" Text="Plot density function" Width="137" Left="0" Top="0" Height="22" IsSelected="True" CanExecute="True" Enabled="True" Syntax="TRUE" IsChecked="True" FontFamily="Segoe UI" FontSize="12" Style="{av:DynamicResource RadioButtonTemplate}" IsEnabled="True" av:Canvas.Left="6" av:Canvas.Top="73"><BSkyRadioButton.CheckedChangeBehaviour><BehaviourCollection Capacity="0" /></BSkyRadioButton.CheckedChangeBehaviour>Plot density function</BSkyRadioButton><BSkyRadioButton GroupName="a" Name="plotdistfun" Text="Plot distribution function" Width="171" Left="0" Top="0" Height="25" IsSelected="False" CanExecute="True" Enabled="True" 