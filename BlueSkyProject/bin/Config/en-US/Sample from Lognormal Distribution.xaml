<BSkyCanvas internalhelpfilename="{x:Null}" Title="Sample from Lognormal Distribution" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="{x:Null}" CanExecute="True" PrereqCommandString="{x:Null}" CommandString="base::set.seed({{seedval}})&#xA;&#xA;{{datasetname}} &lt;- as.data.frame(matrix(stats::rlnorm({{nosamp}}*{{noobserv}}, meanlog={{mean}}, sdlog={{stddev}}), ncol={{noobserv}}))&#xA;rownames({{datasetname}}) &lt;- paste(&quot;sample&quot;, 1:{{nosamp}}, sep='')&#xD;&#xA;colnames({{datasetname}}) &lt;- paste(&quot;obs&quot;, 1:{{noobserv}}, sep='')&#xD;&#xA;{{datasetname}}&lt;- within({{datasetname}}, &#xD;&#xA;{  &#xD;&#xA;#Checking if there is a single row or column&#xD;&#xA;if ( !({{noobserv}} == 1 || {{nosamp}} ==1 ) )&#xD;&#xA;{&#xD;&#xA;if({{sampmean}}) mean &lt;- rowMeans({{datasetname}}[,1:{{noobserv}}]) &#xD;&#xA;if({{sampsums}}) sum &lt;- rowSums({{datasetname}}[,1:{{noobserv}}])&#xD;&#xA;if({{sampstddev}}) sd &lt;- apply({{datasetname}}[,1:{{noobserv}}], 1, sd)&#xD;&#xA;}&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;cat (&quot;We don't calculate sample mean, sum or standard deviation when there is a single row or column&quot;)&#xD;&#xA;}&#xD;&#xA;})&#xD;&#xA;BSkyFormat({{datasetname}}, engNotationSetting=BSkyGetEngNotationSetting(), singleTableOutputHeader=&quot;Samples from Lognormal Distribution&quot;)&#xD;&#xA;&#xD;&#xA;BSkyLoadRefreshDataframe({{datasetname}})" dlgsyntax="base::set.seed({{seedval}})&#xA;&#xA;{{datasetname}} &lt;- as.data.frame(matrix(stats::rlnorm({{nosamp}}*{{noobserv}}, meanlog={{mean}}, sdlog={{stddev}}), ncol={{noobserv}}))&#xA;rownames({{datasetname}}) &lt;- paste(&quot;sample&quot;, 1:{{nosamp}}, sep='')&#xD;&#xA;colnames({{datasetname}}) &lt;- paste(&quot;obs&quot;, 1:{{noobserv}}, sep='')&#xD;&#xA;{{datasetname}}&lt;- within({{datasetname}}, &#xD;&#xA;{  &#xD;&#xA;#Checking if there is a single row or column&#xD;&#xA;if ( !({{noobserv}} == 1 || {{nosamp}} ==1 ) )&#xD;&#xA;{&#xD;&#xA;if({{sampmean}}) mean &lt;- rowMeans({{datasetname}}[,1:{{noobserv}}]) &#xD;&#xA;if({{sampsums}}) sum &lt;- rowSums({{datasetname}}[,1:{{noobserv}}])&#xD;&#xA;if({{sampstddev}}) sd &lt;- apply({{datasetname}}[,1:{{noobserv}}], 1, sd)&#xD;&#xA;}&#xD;&#xA;else&#xD;&#xA;{&#xD;&#xA;cat (&quot;We don't calculate sample mean, sum or standard deviation when there is a single row or column&quot;)&#xD;&#xA;}&#xD;&#xA;})&#xD;&#xA;BSkyFormat({{datasetname}}, engNotationSetting=BSkyGetEngNotationSetting(), singleTableOutputHeader=&quot;Samples from Lognormal Distribution&quot;)&#xD;&#xA;&#xD;&#xA;BSkyLoadRefreshDataframe({{datasetname}})" Helpfile="{x:Null}" splitProcessing="True" DatasetRequired="False" MenuLocation="{x:Null}" OutputDefinition="" Width="250" Command="False" Height="432" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold } .c3 { font-family: 'Courier New' } .c4 { vertical-align: middle } .c5 { vertical-align: top } .c6 { margin: 0px 0px 12px } .c7 { font-style: italic } .c8 { margin: 0px 0px 10px; text-align: center } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&amp;#x200b;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;Density, distribution function, quantile function and random generation for the log normal distribution whose logarithm has mean equal to&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;meanlog&lt;/span&gt;&lt;/span&gt;&amp;#160;and standard deviation equal to&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;sdlog&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Usage&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dlnorm(x, meanlog = 0, sdlog = 1, log = FALSE)&lt;br/&gt;plnorm(q, meanlog = 0, sdlog = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;qlnorm(p, meanlog = 0, sdlog = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;rlnorm(n, meanlog = 0, sdlog = 1)&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c4&quot;&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;x, q&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of quantiles.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of probabilities.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;number of observations. If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;length(n) &amp;gt; 1&lt;/span&gt;&lt;/span&gt;, the length is taken to be the number required.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;meanlog, sdlog&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;mean and standard deviation of the distribution on the log scale with default values of&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;0&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;1&lt;/span&gt;&lt;/span&gt;&amp;#160;respectively.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;log, log.p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE, probabilities p are given as log(p).&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;lower.tail&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE (default), probabilities are&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X ≤ x]&lt;/span&gt;&lt;/span&gt;, otherwise,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X &amp;gt; x]&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Details&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;The log normal distribution has density&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;f(x) = 1/(√(2 π) σ x) e^-((log x - μ)^2 / (2 σ^2))&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;where&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;μ&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;σ&lt;/span&gt;&lt;/span&gt;&amp;#160;are the mean and standard deviation of the logarithm. The mean is&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;E(X) = exp(μ + 1/2 σ^2)&lt;/span&gt;&lt;/span&gt;, the median is&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;med(X) = exp(μ)&lt;/span&gt;&lt;/span&gt;, and the variance&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;Var(X) = exp(2*μ + σ^2)*(exp(σ^2) - 1)&lt;/span&gt;&lt;/span&gt;&amp;#160;and hence the coefficient of variation is&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;sqrt(exp(σ^2) - 1)&lt;/span&gt;&lt;/span&gt;&amp;#160;which is approximately&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;σ&lt;/span&gt;&lt;/span&gt;&amp;#160;when that is small (e.g.,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;σ &amp;lt; 1/2&lt;/span&gt;&lt;/span&gt;).&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Value&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dlnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the density,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;plnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the distribution function,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qlnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the quantile function, and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rlnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;generates random deviates.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The length of the result is determined by&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rlnorm&lt;/span&gt;&lt;/span&gt;, and is the maximum of the lengths of the numerical arguments for the other functions.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The numerical arguments other than&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;are recycled to the length of the result. Only the first elements of the logical arguments are used.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Note&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;The cumulative hazard&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;H(t) = - log(1 - F(t))&lt;/span&gt;&lt;/span&gt;&amp;#160;is&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;-plnorm(t, r, lower = FALSE, log = TRUE)&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(rlnorm, package=stats)" Background="#FFFFFFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.co