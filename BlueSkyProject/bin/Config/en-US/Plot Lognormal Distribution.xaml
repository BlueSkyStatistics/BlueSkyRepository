<BSkyCanvas internalhelpfilename="{x:Null}" Title="Lognormal Distribution Plot" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" customsyntax="{x:Null}" EditableComboBoxName="{x:Null}" RPackages="{x:Null}" CanExecute="True" PrereqCommandString="" CommandString="local(&#xA;{&#xD;&#xA; #Generating the sequence of length 1000, the lower and upper bounds of the sequence are computed using the quantile function of the Log normal distribution  (qlnorm)  with p=0.0005 and p=0.9995 respectively &#xD;&#xA; lowProbBound =0.0005&#xD;&#xA; upperProbBound=0.9995&#xD;&#xA; lowerbound =qlnorm(p=lowProbBound, meanlog={{mean}}, sdlog={{stddev}})&#xD;&#xA; upperbound =qlnorm(p=upperProbBound, meanlog={{mean}}, sdlog={{stddev}})&#xD;&#xA;.x &lt;- seq(lowerbound, upperbound, length.out=1000)  &#xA;&#xA;regionslist=NULL&#xA;if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')) &amp;&amp; !is.na(as.numeric('{{reg2frm}}')) &amp;&amp; !is.na(as.numeric('{{reg2to}}')) )&#xA;{&#xA;    regionslist = list(c({{reg1frm}}, {{reg1to}}), c({{reg2frm}}, {{reg2to}}) )&#xA;}&#xA;else if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')))&#xA;{&#xA;        regionslist = list(c({{reg1frm}}, {{reg1to}}))&#xA;}&#xA;&#xA;if({{plotdenfun}} {{plotdistfun}})&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::dlnorm(.x, meanlog={{mean}}, sdlog={{stddev}}), cdf=FALSE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Density&quot;, &#xA;  main=paste(&quot;Lognormal Distribution:  Mean (log scale)={{mean}}, Standard deviation (log scale)={{stddev}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;else&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::plnorm(.x, meanlog={{mean}}, sdlog={{stddev}}), cdf=TRUE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Cumulative Probability&quot;, &#xA;  main=paste(&quot;Lognormal Distribution:  Mean (log scale)={{mean}}, Standard deviation (log scale)={{stddev}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;&#xA;}&#xA;)" dlgsyntax="local(&#xA;{&#xD;&#xA; #Generating the sequence of length 1000, the lower and upper bounds of the sequence are computed using the quantile function of the Log normal distribution  (qlnorm)  with p=0.0005 and p=0.9995 respectively &#xD;&#xA; lowProbBound =0.0005&#xD;&#xA; upperProbBound=0.9995&#xD;&#xA; lowerbound =qlnorm(p=lowProbBound, meanlog={{mean}}, sdlog={{stddev}})&#xD;&#xA; upperbound =qlnorm(p=upperProbBound, meanlog={{mean}}, sdlog={{stddev}})&#xD;&#xA;.x &lt;- seq(lowerbound, upperbound, length.out=1000)  &#xA;&#xA;regionslist=NULL&#xA;if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')) &amp;&amp; !is.na(as.numeric('{{reg2frm}}')) &amp;&amp; !is.na(as.numeric('{{reg2to}}')) )&#xA;{&#xA;    regionslist = list(c({{reg1frm}}, {{reg1to}}), c({{reg2frm}}, {{reg2to}}) )&#xA;}&#xA;else if(!is.na(as.numeric('{{reg1frm}}')) &amp;&amp; !is.na(as.numeric('{{reg1to}}')))&#xA;{&#xA;        regionslist = list(c({{reg1frm}}, {{reg1to}}))&#xA;}&#xA;&#xA;if({{plotdenfun}} {{plotdistfun}})&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::dlnorm(.x, meanlog={{mean}}, sdlog={{stddev}}), cdf=FALSE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Density&quot;, &#xA;  main=paste(&quot;Lognormal Distribution:  Mean (log scale)={{mean}}, Standard deviation (log scale)={{stddev}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;else&#xA;{&#xA;  RcmdrMisc::plotDistr(.x, stats::plnorm(.x, meanlog={{mean}}, sdlog={{stddev}}), cdf=TRUE, xlab=&quot;x&quot;, &#xA;  ylab=&quot;Cumulative Probability&quot;, &#xA;  main=paste(&quot;Lognormal Distribution:  Mean (log scale)={{mean}}, Standard deviation (log scale)={{stddev}}&quot;),&#xA;regions=regionslist , col=c('#BEBEBE', '#BEBEBE'),&#xA;legend.pos={{toprt}} {{toplt}} {{topcen}})&#xA;}&#xA;&#xA;}&#xA;)" Helpfile="{x:Null}" splitProcessing="True" DatasetRequired="False" MenuLocation="{x:Null}" OutputDefinition="" Width="370" Command="False" Height="455" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold } .c3 { font-family: 'Courier New' } .c4 { vertical-align: middle } .c5 { vertical-align: top } .c6 { margin: 0px 0px 12px } .c7 { font-style: italic } .c8 { margin: 0px 0px 10px; text-align: center } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&amp;#x200b;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;Density, distribution function, quantile function and random generation for the log normal distribution whose logarithm has mean equal to&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;meanlog&lt;/span&gt;&lt;/span&gt;&amp;#160;and standard deviation equal to&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;sdlog&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#160;# We generating the sequence of length 1000, the lower and upper bounds of the sequence are computed using the quantile function of the Log normal distribution &amp;#160;(qlnorm) &amp;#160;with p=0.0005 and p=0.9995 respectively&amp;#160;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Usage&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dlnorm(x, meanlog = 0, sdlog = 1, log = FALSE)&lt;br/&gt;plnorm(q, meanlog = 0, sdlog = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;qlnorm(p, meanlog = 0, sdlog = 1, lower.tail = TRUE, log.p = FALSE)&lt;br/&gt;rlnorm(n, meanlog = 0, sdlog = 1)&lt;br/&gt;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c4&quot;&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;x, q&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of quantiles.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;vector of probabilities.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;number of observations. If&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;length(n) &amp;gt; 1&lt;/span&gt;&lt;/span&gt;, the length is taken to be the number required.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;meanlog, sdlog&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;mean and standard deviation of the distribution on the log scale with default values of&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;0&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;1&lt;/span&gt;&lt;/span&gt;&amp;#160;respectively.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;log, log.p&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE, probabilities p are given as log(p).&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c5&quot;&gt;&lt;td class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;lower.tail&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c5&quot;&gt;&lt;p class=&quot;c6&quot;&gt;logical; if TRUE (default), probabilities are&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X ≤ x]&lt;/span&gt;&lt;/span&gt;, otherwise,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;P[X &amp;gt; x]&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Details&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;The log normal distribution has density&lt;/p&gt;&lt;p class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;f(x) = 1/(√(2 π) σ x) e^-((log x - μ)^2 / (2 σ^2))&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;where&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;μ&lt;/span&gt;&lt;/span&gt;&amp;#160;and&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;σ&lt;/span&gt;&lt;/span&gt;&amp;#160;are the mean and standard deviation of the logarithm. The mean is&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;E(X) = exp(μ + 1/2 σ^2)&lt;/span&gt;&lt;/span&gt;, the median is&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;med(X) = exp(μ)&lt;/span&gt;&lt;/span&gt;, and the variance&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;Var(X) = exp(2*μ + σ^2)*(exp(σ^2) - 1)&lt;/span&gt;&lt;/span&gt;&amp;#160;and hence the coefficient of variation is&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;sqrt(exp(σ^2) - 1)&lt;/span&gt;&lt;/span&gt;&amp;#160;which is approximately&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;σ&lt;/span&gt;&lt;/span&gt;&amp;#160;when that is small (e.g.,&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;σ &amp;lt; 1/2&lt;/span&gt;&lt;/span&gt;).&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Value&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dlnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the density,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;plnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the distribution function,&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;qlnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;gives the quantile function, and&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rlnorm&lt;/span&gt;&lt;/span&gt;&amp;#160;generates random deviates.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The length of the result is determined by&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;for&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;rlnorm&lt;/span&gt;&lt;/span&gt;, and is the maximum of the lengths of the numerical arguments for the other functions.&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;The numerical arguments other than&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&amp;#160;are recycled to the length of the result. Only the first elements of the logical arguments are used.&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Note&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c1&quot;&gt;The cumulative hazard&amp;#160;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;H(t) = - log(1 - F(t))&lt;/span&gt;&lt;/span&gt;&amp;#160;is&amp;#160;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;-plnorm(t, r, lower = FALSE, log = TRUE)&lt;/span&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(dlnorm , package=stats)" Background="#FFFFFFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"><BSkyLabel Name="meanlbl" Text="Mean (log scale)" Width="100" Height="36" Left="2" Top="3" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="2" av:Canvas.Top="3">Mean (log scale)</BSkyLabel><BSkyLabel Name="stddevi" Text="Standard deviation (log scale)" Width="166" Height="35" Left="2" Top="70" CanExecute="False" FontFamily="Segoe UI" Fo