<BSkyCanvas internalhelpfilename="{assembly:Null}" Title="ICC Plots" ModelClasses="{assembly:Null}" StatusTextBoxName="{assembly:Null}" EditableComboBoxName="modelselector1" RPackages="eRm" CanExecute="True" PrereqCommandString="BSkyGetAvailableModels(c(&quot;Rm&quot;, &quot;dRm&quot;))" CommandString="plotICC({{modelselector1}}, item.subset = &quot;all&quot;, empICC=list(&quot;raw&quot;,type=&quot;b&quot;,col=&quot;blue&quot;,lty=&quot;dotted&quot;, empCI = NULL,&#xA;   mplot = NULL), xlim = c({{range1}}), ylim = c(0, 1),&#xA;   xlab = &quot;Latent Dimension&quot;, ylab = &quot;Probability to Solve&quot;, main=NULL,&#xA;   col = NULL, lty = 1, legpos = &quot;left&quot;, ask = TRUE)&#xA;" dlgsyntax="plotICC({{modelselector1}}, item.subset = &quot;all&quot;, empICC=list(&quot;raw&quot;,type=&quot;b&quot;,col=&quot;blue&quot;,lty=&quot;dotted&quot;, empCI = NULL,&#xA;   mplot = NULL), xlim = c({{range1}}), ylim = c(0, 1),&#xA;   xlab = &quot;Latent Dimension&quot;, ylab = &quot;Probability to Solve&quot;, main=NULL,&#xA;   col = NULL, lty = 1, legpos = &quot;left&quot;, ask = TRUE)&#xA;" Helpfile="{assembly:Null}" splitProcessing="False" MenuLocation="{assembly:Null}" OutputDefinition="" Width="420" Command="False" Height="180" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { font-weight: bold; font-size: 14.6666666666667px; line-height: 1.14999997615814; font-family: 'Arial' } .c3 { background-color: Transparent; color: Black; font-family: 'Times New Roman'; font-size: 16px; font-style: normal; font-weight: normal; text-align: left; text-decoration: none; text-indent: 0px } .c4 { margin: 0px 0px 12px; font-size: 16.5px } .c5 { margin: 0px 0px 10px; line-height: 1.14999997615814; font-size: 11px; font-family: 'Arial' } .c6 { font-weight: bold; font-size: 14.6666666666667px; font-family: 'Arial' } .c7 { font-weight: bold; font-size: 14.6666660308838px; font-family: 'Arial' } .c8 { margin: 0px 0px 11px; font-size: 12.87px; font-weight: bold } .c9 { font-size: 14.6666666666667px; font-family: 'Arial' } .c10 { color: Black; text-decoration: none; background-color: Transparent; font-family: 'Consolas'; font-size: 13.33px; font-style: normal; font-weight: normal; text-align: left; text-indent: 0px } .c11 { font-family: 'Times New Roman'; font-size: 16px } .c12 { vertical-align: middle; background-color: Transparent; color: Black } .c13 { vertical-align: top; background-color: Transparent; color: Black } .c14 { font-family: 'Courier New'; background-color: Transparent; color: Black } .c15 { font-family: 'Times New Roman' } .c16 { margin: 0px 0px 12px; background-color: Transparent; color: Black } .c17 { background-color: Transparent; color: Black } .c18 { color: Black; text-decoration: underline; background-color: Transparent; font-family: 'Courier New' } .c19 { vertical-align: middle } .c20 { vertical-align: top } .c21 { margin: 0px 0px 12px } .c22 { font-size: 16px; font-family: 'Arial'; font-weight: bold } .c23 { color: Black; font-family: 'Times New Roman'; font-size: 16px; font-style: normal; font-weight: normal; text-align: left; text-decoration: none; text-indent: 0px } .c24 { font-family: 'Courier New'; background-color: Transparent; color: Black; font-size: 16px; font-style: normal; font-weight: normal; text-align: left; text-decoration: none; text-indent: 0px } .c25 { color: Black; text-decoration: underline; background-color: Transparent; font-family: 'Courier New'; font-size: 16px; font-style: normal; font-weight: normal; text-align: left; text-indent: 0px } .c26 { background-color: White; color: Black; font-family: 'Times New Roman'; font-size: 16px; font-style: normal; font-weight: normal; text-align: left; text-decoration: none; text-indent: 0px } .c27 { margin: 0px 0px 10px; background-color: Transparent; color: Black; font-family: 'Times New Roman'; font-size: 16px; font-style: normal; font-weight: normal; text-align: left; text-decoration: none; text-indent: 0px } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Description:&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;div&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&amp;#x200b;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Plot functions for visualizing the item characteristic curves&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;Usage:&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;div&gt;&amp;#x200b;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;## S3 method for class 'Rm'&lt;br/&gt;plotICC(object, item.subset = &amp;quot;all&amp;quot;, empICC = NULL, empCI = NULL,&lt;br/&gt; &amp;#160;&amp;#160;mplot = NULL, xlim = c(-4, 4), ylim = c(0, 1),&lt;br/&gt; &amp;#160;&amp;#160;xlab = &amp;quot;Latent Dimension&amp;quot;, ylab = &amp;quot;Probability to Solve&amp;quot;, main=NULL,&lt;br/&gt; &amp;#160;&amp;#160;col = NULL, lty = 1, legpos = &amp;quot;left&amp;quot;, ask = TRUE, ...)&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/div&gt;&lt;p class=&quot;c5&quot;&gt;&lt;span&gt;&lt;span class=&quot;c7&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;div class=&quot;c8&quot;&gt;&lt;span&gt;&lt;span class=&quot;c9&quot;&gt;Arguments&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div class=&quot;c8&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&amp;#x200b;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;object:&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;object of class&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Rm&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;or&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dRm&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&amp;#x200b;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;item.subset:&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Subset of items to be plotted. Either a numeric vector indicating the column in&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;X&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;or a character vector indiciating the column name. If&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;all&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;(default), all items are plotted.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;empICC&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Plotting the empirical ICCs for objects of class&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dRm&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;. If&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;empICC=NULL&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;(the default) the empirical ICC is not drawn. Otherwise,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;empICC&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;must be specified as a list where the first element must be one of&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;raw&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;loess&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;tukey&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;kernel&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;. The other optional elements are&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;smooth&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;(numeric),&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;type&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;(line type for empirical ICCs, useful values are&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;p&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;(default),&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;l&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;, &amp;#160;and&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;b&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;, see graphics parameter&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;type&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;in&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;a href=&quot;http://127.0.0.1:31865/library/eRm/help/plot.default&quot; class=&quot;c10&quot;&gt;&lt;span&gt;&lt;span class=&quot;c11&quot;&gt;plot.default&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;),&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;pch&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;col&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;, and&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;lty&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;, plotting ‘character’, colour and linetype (see&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;a href=&quot;http://127.0.0.1:31865/library/eRm/help/par&quot; class=&quot;c10&quot;&gt;&lt;span&gt;&lt;span class=&quot;c11&quot;&gt;par&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;). See details and examples below.&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&amp;#x200b;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;empCI:&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Plotting confidence intervals for the the empirical ICCs. If&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;empCI=NULL&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;(the default) no confidence intervals are drawn. Otherwise, by specifying&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;empCI&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;as a list gives ‘exact’ confidence intervals for each point of the empirical ICC. The optional elements of this list are&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;gamma&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;, the confidence level,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;col&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;, colour, and&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;lty&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;, line type. If&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;empCI&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;is specified as an empty list, the default values&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;empCI=list(gamma=0.95,col=&amp;quot;red&amp;quot;,lty=&amp;quot;dotted&amp;quot;)&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;will be used.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;mplot:&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;if&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;NULL&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;the default setting is in effect. For models of class&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;dRm&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;this is&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;mplot = TRUE&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;, i.e., the ICCs for up to 4 items are plotted in one figure. For&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Rm&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;models the default is&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;FALSE&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;(each item in one figure) but may be set to&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;TRUE&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&amp;#x200b;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;xlab: Label of x axis&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;ylab: Label of y axis&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;xlim: Range of person parameters&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;ylim: Range of probability to solve&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;legpos:&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;Position of the legend with possible values&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;bottomright&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;bottom&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;bottomleft&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;left&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;topleft&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;top&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;topright&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;right&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;and&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;quot;center&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;. If&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;FALSE&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c3&quot;&gt;&amp;#160;no legend is displayed.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;table class=&quot;c3&quot;&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c12&quot;&gt;&lt;tr class=&quot;c13&quot;&gt;&lt;td class=&quot;c13&quot;&gt;&lt;span class=&quot;c14&quot;&gt;&lt;span class=&quot;c15&quot;&gt;ask:&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c16&quot;&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;If&amp;#160;&lt;span&gt;&lt;span class=&quot;c17&quot;&gt;TRUE&lt;/span&gt;&lt;/span&gt;&amp;#160;(the default) and the&amp;#160;&lt;span&gt;&lt;span class=&quot;c17&quot;&gt;R&lt;/span&gt;&lt;/span&gt;&amp;#160;session is interactive the user is asked for input, before a new figure is drawn.&amp;#160;&lt;span&gt;&lt;span class=&quot;c17&quot;&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;FALSE&lt;/span&gt;&lt;/span&gt;&amp;#160;is only useful if automated figure export is in effect, e.g., when using&amp;#160;&lt;a href=&quot;http://127.0.0.1:31865/library/eRm/help/Sweave&quot; class=&quot;c18&quot;&gt;&lt;span&gt;&lt;span class=&quot;c15&quot;&gt;Sweave&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;.&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c19&quot;&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c13&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&amp;#x200b;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/div&gt;&lt;table&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c19&quot;&gt;&lt;tr class=&quot;c20&quot;&gt;&lt;td class=&quot;c20&quot;&gt;&lt;p class=&quot;c21&quot;&gt;&lt;span&gt;&lt;span class=&quot;c22&quot;&gt;Details&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;Empirical ICCs for objects of class&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;dRm&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;&amp;#160;can be plotted using the option&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;empICC&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;, a list where the first element specifies the type of calculation of the empirical values. If&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;empICC=list(&amp;quot;raw&amp;quot;, other specifications)&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;&amp;#160;relative frequencies of the positive responses are calculated for each rawscore group and plotted at the position of the corresponding person parameter. The other options use the default versions of various smoothers:&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;&amp;quot;tukey&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;&amp;#160;(see&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;a href=&quot;http://127.0.0.1:31865/library/eRm/help/smooth&quot; class=&quot;c25&quot;&gt;smooth&lt;/a&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;),&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;&amp;quot;loess&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;&amp;#160;(see&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;a href=&quot;http://127.0.0.1:31865/library/eRm/help/loess&quot; class=&quot;c25&quot;&gt;loess&lt;/a&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;), and&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;&amp;quot;kernel&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;&amp;#160;(see&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;a href=&quot;http://127.0.0.1:31865/library/eRm/help/ksmooth&quot; class=&quot;c25&quot;&gt;ksmooth&lt;/a&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;). For&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;&amp;quot;loess&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;&amp;#160;and&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;&amp;quot;kernel&amp;quot;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;&amp;#160;a further element,&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;smooth&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;, may be specified to control the span (default is 0.75) or the bandwith (default is 0.5), respectively. For example, the specification could be&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;empirical = list(&amp;quot;loess&amp;quot;, smooth=0.9)&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;&amp;#160;or&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c24&quot;&gt;empirical = list(&amp;quot;kernel&amp;quot;,smooth=2)&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c23&quot;&gt;. Higher values result in smoother estimates of the empirical ICCs.&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c26&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c27&quot;&gt;The optional confidence intervals are obtained by a procedure first given in Clopper and Pearson (1934) based on the beta distribution (see&amp;#160;&lt;a href=&quot;http://127.0.0.1:31865/library/eRm/help/binom.test&quot; class=&quot;c18&quot;&gt;binom.test&lt;/a&gt;).&lt;/p&gt;&lt;p class=&quot;c27&quot;&gt;&amp;#x200b;&lt;/p&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c22&quot;&gt;Package&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c11&quot;&gt;eRm&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&lt;span class=&quot;c8&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;/body&gt;&lt;/html&gt;" RHelpText="help(plotPImap, package=&quot;eRm&quot;)" Background="#FFFFFFFF" xmlns="clr-namespace:BSky.Controls;assembly=BSky.Controls" xmlns:assembly="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"><BSkyEditableComboBox Name="modelselector1" DefaultSelection="" NothingSelected="{assembly:Null}" prefixSelectedValue="" Width="145" Height="30" Left="11.9999999999999" Top="81.3333333333334" CanExecute="True" Enabled="True" Syntax="%%VALUE%%" IsEditable="True" Text="" IsEnabled="True" av:Canvas.Left="11.9999999999999" av:Canvas.Top="81.3333333333334"><BSkyEditableComboBox.ComboBoxEntries><assembly:Null /></BSkyEditableComboBox.ComboBoxEntries><BSkyEditableComboBox.SelectionChangeBehaviour><BehaviourCollection Capacity="0" /></BSkyEditableComboBox.SelectionChangeBehaviour></BSkyEditableComboBox><BSkyLabel Name="lab2" Text="If no models are available, you need to create a model first by selecting " Width="420.333333333333" Height="26.6666666666667" Left="7.00000000000006" Top="29.3333333333333" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="7.00000000000006" av:Canvas.Top="29.3333333333333" xml:space="preserve">If no models are available, you need to create a model first by selecting </BSkyLabel><BSkyLabel Name="labrange" Text="Optionally specify range for the X axis in format a,b for e.g. -1,6" Width="343.666666666667" Height="30.3333333333334" Left="7" Top="122" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="7" av:Canvas.Top="122">Optionally specify range for the X axis in format a,b for e.g. -1,6</BSkyLabel><BSkyTextBox PrefixTxt="{assembly:Null}" OverWriteExistingVariables="False" OverwriteSettings="DontPrompt" Name="range1" Width="50" Height="30" Left="352" Top="122.333333333333" SubstituteSettings="TextAsIs" CanExecute="True" Enabled="True" Syntax="%%VALUE%%" TextWrapping="Wrap" AcceptsTab="True" VerticalScrollBarVisibility="Auto" FontFamily="Segoe UI" FontSize="12" VerticalContentAlignment="Center" Template="{av:DynamicResource TextBoxBaseControlTemplate}" AllowDrop="True" IsEnabled="True" av:Canvas.Left="352" av:Canvas.Top="122.333333333333" xml:space="preserve"><BSkyTextBox.TextChangedBehaviour><BehaviourCollection Capacity="0" /></BSkyTextBox.TextChangedBehaviour><BSkyTextBox.Style><av:Style TargetType="av:TextBox"><av:Style.Resources><av:ResourceDictionary /></av:Style.Resources><av:Setter Property="av:UIElement.Effect"><av:Setter.Value><av:DropShadowEffect Color="#FFF5F5DC" /></av:Setter.Value></av:Setter></av:Style></BSkyTextBox.Style></BSkyTextBox><BSkyLabel Name="lab1" Text="Select a model (models of class Rm and dRm supported):" Width="318" Height="25.6666666666667" Left="6.66666666666663" Top="8" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="6.66666666666663" av:Canvas.Top="8">Select a model (models of class Rm and dRm supported):</BSkyLabel><BSkyLabel Name="lab3" Text="Model statistics -&gt;Simple Rasch Model and choose CML estimation." Width="404" Height="32.3333333333333" Left="7" Top="49.6666666666667" CanExecute="False" FontFamily="Segoe UI" FontSize="12" av:Canvas.Left="7" av:Canvas.Top="49.6666666666667">Model statistics -&gt;Simple Rasch Model and choose CML estimation.</BSkyLabel></BSkyCanvas>