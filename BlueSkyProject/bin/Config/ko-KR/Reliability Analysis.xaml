<BSkyCanvas internalhelpfilename="{x:Null}" Title="Reliability Analysis" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" RPackages="psych" CanExecute="True" PrereqCommandString="{x:Null}" CommandString="local({&#xA;reliabilityRes &lt;-alpha(  {{%DATASET%}}[,{{tvarbox1}}],keys={{tvarbox2}})&#xA;&#xA;colnames(reliabilityRes$total)[1]=&quot;Cronbach\'s Alpha&quot;&#xA;colnames(reliabilityRes$total)[2]=&quot;Cronbach\'s Standardized Alpha&quot;&#xA;colnames(reliabilityRes$total)[3]=&quot;Guttman\'s Lambda 6&quot;&#xA;BSkyFormat(reliabilityRes$total,decimalDigitsRounding=4,singleTableOutputHeader=&quot;Reliability Statistics&quot;)&#xA;&#xA;colnames(reliabilityRes$alpha.drop)[1]=&quot;Cronbach\'s Alpha&quot;&#xA;colnames(reliabilityRes$alpha.drop)[2]=&quot;Cronbach\'s Standardized Alpha&quot;&#xA;colnames(reliabilityRes$alpha.drop)[3]=&quot;Guttman\'s Lambda 6&quot;&#xA;BSkyFormat(reliabilityRes$alpha.drop,decimalDigitsRounding=4,singleTableOutputHeader=&quot;Reliability if an item is dropped&quot;)&#xA;&#xA;BSkyFormat(reliabilityRes$item.stats,decimalDigitsRounding=4,singleTableOutputHeader=&quot;Item Statistics&quot;)&#xA;BSkyFormat(reliabilityRes$response.freq,decimalDigitsRounding=4,singleTableOutputHeader=&quot;Non missing response frequency for each item&quot;)&#xA;})" dlgsyntax="local({&#xA;reliabilityRes &lt;-alpha(  {{%DATASET%}}[,{{tvarbox1}}],keys={{tvarbox2}})&#xA;&#xA;colnames(reliabilityRes$total)[1]=&quot;Cronbach\'s Alpha&quot;&#xA;colnames(reliabilityRes$total)[2]=&quot;Cronbach\'s Standardized Alpha&quot;&#xA;colnames(reliabilityRes$total)[3]=&quot;Guttman\'s Lambda 6&quot;&#xA;BSkyFormat(reliabilityRes$total,decimalDigitsRounding=4,singleTableOutputHeader=&quot;Reliability Statistics&quot;)&#xA;&#xA;colnames(reliabilityRes$alpha.drop)[1]=&quot;Cronbach\'s Alpha&quot;&#xA;colnames(reliabilityRes$alpha.drop)[2]=&quot;Cronbach\'s Standardized Alpha&quot;&#xA;colnames(reliabilityRes$alpha.drop)[3]=&quot;Guttman\'s Lambda 6&quot;&#xA;BSkyFormat(reliabilityRes$alpha.drop,decimalDigitsRounding=4,singleTableOutputHeader=&quot;Reliability if an item is dropped&quot;)&#xA;&#xA;BSkyFormat(reliabilityRes$item.stats,decimalDigitsRounding=4,singleTableOutputHeader=&quot;Item Statistics&quot;)&#xA;BSkyFormat(reliabilityRes$response.freq,decimalDigitsRounding=4,singleTableOutputHeader=&quot;Non missing response frequency for each item&quot;)&#xA;})" Helpfile="" splitProcessing="True" MenuLocation="{x:Null}" OutputDefinition="{x:Null}" Width="420" Command="False" Height="405" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { font-family: 'Times New Roman','Times New Roman'; color: #333333; line-height: normal; font-size: 16px } .c3 { margin: 0px 0px 0px 40px; list-style-type: disc } .c4 { margin: 0px 0px 11px; line-height: normal; font-size: 14.6666666666667px; font-family: 'Calibri','Arial'; color: #333333; background-color: White } .c5 { font-family: 'Times New Roman','Times New Roman' } .c6 { font-family: 'Times New Roman','Times New Roman'; color: Black } .c7 { margin: 0px 0px 11px; line-height: normal; font-size: 16px; font-family: 'Times New Roman','Times New Roman'; background-color: White } .c8 { font-family: 'Times New Roman','Times New Roman'; color: #333333 } .c9 { margin: 0px 0px 11px 48px; font-size: 14.6666666666667px; font-family: 'Calibri','Arial'; line-height: normal; text-indent: -24px; background-color: White } .c10 { font-family: 'Symbol'; color: #333333 } .c11 { font-size: 9.33333333333333px; font-family: 'Times New Roman' } .c12 { font-family: 'Times New Roman','Times New Roman'; color: #333333; background-color: White } .c13 { margin: 0px 0px 10px; line-height: normal; font-size: 16px; font-family: 'Times New Roman','Times New Roman'; background-color: White } .c14 { margin: 0px 0px 10px; line-height: normal; font-size: 14.6666666666667px; font-family: 'Calibri','Arial'; color: #333333; background-color: White } .c15 { margin: 0px 0px 11px; line-height: 1.07; font-size: 16px; font-family: 'Times New Roman','Times New Roman' } .c16 { border-radius: 4px; font-family: 'Courier New' } .c17 { line-height: 1.07; font-family: 'Times New Roman','Times New Roman'; color: #333333 } .c18 { font-family: 'Times New Roman','Times New Roman'; font-weight: bold } .c19 { margin: 0px 0px 10px; line-height: normal; font-size: 16px; font-family: 'Times New Roman','Times New Roman' } .c20 { font-weight: bold } .c21 { font-size: 12px; font-family: 'Times New Roman','Times New Roman' } .c22 { margin: 0px; line-height: normal; font-size: 16px; font-family: 'Times New Roman','Times New Roman' } .c23 { margin: 0px 0px 11px; line-height: normal; font-size: 16px; font-family: 'Times New Roman','Times New Roman' } .c24 { font-family: 'Times New Roman','Times New Roman'; color: Black; font-weight: bold } .c25 { border-color: Black; border-style: solid; border-width: 0px } .c26 { width: 135px } .c27 { width: 483px } .c28 { vertical-align: middle } .c29 { border-color: Black; border-style: solid; border-width: 0px; padding: 1px; vertical-align: top; width: 135px } .c30 { border-color: Black; border-style: solid; border-width: 0px; padding: 1px; vertical-align: top; width: 483px } .c31 { margin: 0px 0px 15px; font-size: 18px; font-weight: bold; font-family: 'Times New Roman','Times New Roman' } .c32 { font-size: 14.6666666666667px; color: Black } .c33 { margin: 0px; font-size: 16px; font-family: 'Times New Roman','Times New Roman' } .c34 { border-color: Black; border-style: solid; border-width: 0px; padding: 1px; vertical-align: top } .c35 { font-family: 'Times New Roman','Times New Roman'; font-weight: bold; line-height: normal; font-size: 16px } .c36 { font-size: 14.6666666666667px; line-height: 1.07; font-family: 'Times New Roman','Times New Roman' } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c2&quot;&gt;Reliability statistics table gives you&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;ul class=&quot;c3&quot;&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;Cronbach’s alpha: Cronbach’s α (values ≥ .7 or .8 indicate good reliability; Kline (1999))&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;std.alpha:&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c6&quot;&gt;The standarized alpha based upon the correlations&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;Guttman’s lambda 6 (calculated from the squared multiple correlation or ‘smc’)&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;average_r: average inter-item correlation (this is used to calculate std.alpha)&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;mean: scale mean (the mean of the means of all individuals)&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;sd: scale standard deviation&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;/ul&gt;&lt;p class=&quot;c7&quot;&gt;&lt;span class=&quot;c8&quot;&gt;Interpreting reliability if an item is dropped&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c9&quot;&gt;&lt;span class=&quot;c10&quot;&gt;&amp;#183;&lt;span&gt;&lt;span class=&quot;c11&quot;&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;Refers to the overall&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c8&quot;&gt;Cronbach’s alpha&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;&amp;#160;when that particular item (in the row) has been dropped/deleted&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c8&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;ul class=&quot;c3&quot;&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;When the raw Cronbach’s alpha values of any row is greater than the overall α in the reliability statistics table above, this means that dropping that particular item in the row will increase the overall Cronbach’s alpha of the scale.&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;The other columns of this table refer to how the other statistics will change if that particular item has been dropped/deleted.&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;/ul&gt;&lt;p class=&quot;c13&quot;&gt;&lt;span class=&quot;c8&quot;&gt;How to interpret ‘Item statistics’?&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;ul class=&quot;c3&quot;&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;raw.r: The correlation of each item with the total score, not corrected for item overlap.&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;r.drop: item-total correlation without that item itself (i.e., item-rest correlation or corrected item-total correlation); low item-total correlations indicate that that item doesn’t correlate well with the scale overall&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;r.cor: item-total correlation corrected for item overlap and scale reliability&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c14&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;mean and sd: mean and sd of the scale if that item is dropped&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c14&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;All items should correlate the the total score, so we’re looking for items that don’t correlate with the overall score from the scale. If r.drop values are less than about .3, it means that particular item doesn’t correlate very well with the scale overall.&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;/ul&gt;&lt;p class=&quot;c13&quot;&gt;&lt;span class=&quot;c8&quot;&gt;How to interpret the final frequency table?&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;ul class=&quot;c3&quot;&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;This table tells us what percentage of people gave each response to each of the items (i.e., if you have a 5-point scale, then it tells you how many percent of responses were 1, 2, 3, 4, or 5).&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li class=&quot;c4&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span class=&quot;c5&quot;&gt;This helps you check the distribution of responses and whether everyone is giving the same responses (which will lead to low reliability).&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;/ul&gt;&lt;p class=&quot;c15&quot;&gt;&lt;span&gt;&lt;span class=&quot;c12&quot;&gt;If your scale contains items that are reversed scored, you need to specify them. The&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c16&quot;&gt;&lt;span class=&quot;c17&quot;&gt;keys&lt;/span&gt;&lt;/span&gt;&lt;span class=&quot;c12&quot;&gt;&amp;#160;argument allows you to R&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c15&quot;&gt;&amp;#160;&lt;/p&gt;&lt;p class=&quot;c15&quot;&gt;&lt;span class=&quot;c18&quot;&gt;Package: psych&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c15&quot;&gt;&lt;span class=&quot;c18&quot;&gt;Function: alpha&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c19&quot;&gt;&lt;span class=&quot;c20&quot;&gt;&lt;span class=&quot;c5&quot;&gt;Description&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c19&quot;&gt;&lt;span class=&quot;c5&quot;&gt;Find two estimates of reliability: Cronbach's alpha and Guttman's Lambda 6.&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c19&quot;&gt;&lt;span class=&quot;c20&quot;&gt;&lt;span class=&quot;c21&quot;&gt;&amp;#160;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c19&quot;&gt;&lt;span class=&quot;c18&quot;&gt;Usage&amp;#160;&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;alpha(x, keys=NULL,cumulative=FALSE, title=NULL, max=10,na.rm = TRUE,&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;&amp;#160;&amp;#160;&amp;#160;check.keys=FALSE,n.iter=1,delete=TRUE,use=&amp;quot;pairwise&amp;quot;,warnings=TRUE,n.obs=NULL)&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c24&quot;&gt;Arguments&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;table class=&quot;c25&quot;&gt;&lt;col class=&quot;c26&quot;/&gt;&lt;col class=&quot;c27&quot;/&gt;&lt;tbody class=&quot;c28&quot;&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;x&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;A data.frame or matrix of data, or a covariance or correlation matrix&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;keys&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;If some items are to be reversed keyed, then either specify the direction of all items or just a vector of which items to reverse&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;title&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;Any text string to identify this run&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;cumulative&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;should means reflect the sum of items or the mean of the items. The default value is means.&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;max&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;the number of categories/item to consider if reporting category frequencies. Defaults to 10, passed to link{response.frequencies}&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;na.rm&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;The default is to remove missing values and find pairwise correlations&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;check.keys&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;if TRUE, then find the first principal component and reverse key items with negative loadings. Give a warning if this happens.&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;n.iter&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;Number of iterations if bootstrapped confidence intervals are desired&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;delete&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;Delete items with no variance and issue a warning&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;use&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;Options to pass to the cor function: &amp;quot;everything&amp;quot;, &amp;quot;all.obs&amp;quot;, &amp;quot;complete.obs&amp;quot;, &amp;quot;na.or.complete&amp;quot;, or &amp;quot;pairwise.complete.obs&amp;quot;. The default is &amp;quot;pairwise&amp;quot;&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;warnings&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;By default print a warning and a message that items were reversed. Suppress the message if warnings = FALSE&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c29&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;n.obs&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c30&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;If using correlation matrices as input, by specify the number of observations, we can find confidence intervals&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;div class=&quot;c31&quot;&gt;&lt;span class=&quot;c32&quot;&gt;Details&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;p class=&quot;c33&quot;&gt;&lt;span class=&quot;c32&quot;&gt;Alpha is one of several estimates of the internal consistency reliability of a test.&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c24&quot;&gt;Value&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;table class=&quot;c25&quot;&gt;&lt;col/&gt;&lt;col/&gt;&lt;tbody class=&quot;c28&quot;&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;total&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;a list containing&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;raw_alpha&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;alpha based upon the covariances&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;std.alpha&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;The standarized alpha based upon the correlations&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;G6(smc)&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;Guttman's Lambda 6 reliability&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;average_r&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;The average interitem correlation&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;mean&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;For data matrices, the mean of the scale formed by summing the items&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;sd&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;For data matrices, the standard deviation of the total score&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;alpha.drop&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;A data frame with all of the above for the case of each item being removed one by one.&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;item.stats&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;A data frame including&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;n&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;number of complete cases for the item&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;raw.r&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;The correlation of each item with the total score, not corrected for item overlap.&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;std.r&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;The correlation of each item with the total score (not corrected for item overlap) if the items were all standardized&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;r.cor&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;Item whole correlation corrected for item overlap and scale reliability&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;r.drop&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;Item whole correlation for this item against the scale without this item&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;mean&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;for data matrices, the mean of each item&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;sd&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;For data matrices, the standard deviation of each item&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c22&quot;&gt;&lt;span class=&quot;c6&quot;&gt;response.freq&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p class=&quot;c23&quot;&gt;&lt;span class=&quot;c6&quot;&gt;For data matrices, the frequency of each item response (if less than 20)&lt;span&gt;&amp;#x200b;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr class=&quot;c28&quot;&gt;&lt;td class=&quot;c34&quot;&gt;&lt;p clas