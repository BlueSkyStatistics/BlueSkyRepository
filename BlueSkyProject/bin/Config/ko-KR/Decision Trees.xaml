<BSkyCanvas internalhelpfilename="{x:Null}" Title="Tree Analysis" ModelClasses="{x:Null}" StatusTextBoxName="{x:Null}" RPackages="rpart; rpart.plot; rattle" CanExecute="True" PrereqCommandString="{x:Null}" CommandString="#Generate a new tree model if the model does not exist of the user has chosen the option to generate a model&#xA;if(!{{TreeGenChkbox}} || !exists(&quot;{{TxtTreeName}}&quot;))&#xA;{&#xA;            {{TxtTreeName}} = rpart({{dependent}}~{{independent}}, method={{RadioTreeMethod}}, data = {{%DATASET%}}, control = rpart.control(minsplit={{TxtMinSplit}}, cp ={{TxtCP}}))&#xA;}&#xA;&#xA;#If the user selected the option to prune the tree, prune it&#xA;if({{PruneTreeChkbox}})&#xA;{&#xA;      if({{PruneCPChkbox}})&#xA;      {&#xA;          bsky_tree = prune({{TxtTreeName}}, cp = {{TxtTreeName}}$cptable[which.min({{TxtTreeName}}$cptable[,&quot;xerror&quot;]),&quot;CP&quot;])&#xA;      }&#xA;      else&#xA;      {&#xA;         # prune the tree with the complexity parameter (cp) that the user provided&#xA;         bsky_tree = prune({{TxtTreeName}}, cp = {{TxtCPPrune}})&#xA;      }&#xA;}&#xA;else&#xA;{&#xA;    #Storing the model specified into bsky_tree for summaries below&#xA;    bsky_tree = {{TxtTreeName}}&#xA;}&#xA;&#xA;if({{PruneTreeChkbox}})&#xA;{&#xA;cat(&quot;Statistics for the pruned model are displayed&quot;)&#xA;}&#xA;&#xA;BSkyFormat(bsky_tree$method, singleTableOutputHeader=c(&quot;Tree Type&quot;))&#xA;BSkyFormat(as.data.frame(bsky_tree$control), singleTableOutputHeader=c(&quot;Control Parameters&quot;))&#xA;BSkyFormat(as.data.frame(bsky_tree$cptable), singleTableOutputHeader=c(&quot;CP Table&quot;))&#xA;BSkyFormat(as.data.frame(bsky_tree$variable.importance), singleTableOutputHeader=c(&quot;Variable of Importance&quot;))&#xA;&#xA;if({{PlotCVChkbox}})  { plotcp(bsky_tree, main =&quot;Plot of the Complexity Parameter table&quot;)  }                             #plot cp table&#xA;if({{PlotRSQRChkbox}}) { rsq.rpart(bsky_tree) }                       #appropriate for the &quot;anova&quot; method&#xA;bsky_temp = prp(bsky_tree)   &#xA;&#xA;if({{PlotRattle}}) { fancyRpartPlot(bsky_tree) }                         # A better looking plotting from rattle  package&#xA;rm(bsky_tree)&#xA;&#xA;#Useful commands&#xA;#plot(bsky_tree)&#xA;#Summarize the model&#xA;#summary(bsky_tree)&#xA;#Print the cp table&#xA;#printcp(bsky_tree)&#xA;" dlgsyntax="#Generate a new tree model if the model does not exist of the user has chosen the option to generate a model&#xA;if(!{{TreeGenChkbox}} || !exists(&quot;{{TxtTreeName}}&quot;))&#xA;{&#xA;            {{TxtTreeName}} = rpart({{dependent}}~{{independent}}, method={{RadioTreeMethod}}, data = {{%DATASET%}}, control = rpart.control(minsplit={{TxtMinSplit}}, cp ={{TxtCP}}))&#xA;}&#xA;&#xA;#If the user selected the option to prune the tree, prune it&#xA;if({{PruneTreeChkbox}})&#xA;{&#xA;      if({{PruneCPChkbox}})&#xA;      {&#xA;          bsky_tree = prune({{TxtTreeName}}, cp = {{TxtTreeName}}$cptable[which.min({{TxtTreeName}}$cptable[,&quot;xerror&quot;]),&quot;CP&quot;])&#xA;      }&#xA;      else&#xA;      {&#xA;         # prune the tree with the complexity parameter (cp) that the user provided&#xA;         bsky_tree = prune({{TxtTreeName}}, cp = {{TxtCPPrune}})&#xA;      }&#xA;}&#xA;else&#xA;{&#xA;    #Storing the model specified into bsky_tree for summaries below&#xA;    bsky_tree = {{TxtTreeName}}&#xA;}&#xA;&#xA;if({{PruneTreeChkbox}})&#xA;{&#xA;cat(&quot;Statistics for the pruned model are displayed&quot;)&#xA;}&#xA;&#xA;BSkyFormat(bsky_tree$method, singleTableOutputHeader=c(&quot;Tree Type&quot;))&#xA;BSkyFormat(as.data.frame(bsky_tree$control), singleTableOutputHeader=c(&quot;Control Parameters&quot;))&#xA;BSkyFormat(as.data.frame(bsky_tree$cptable), singleTableOutputHeader=c(&quot;CP Table&quot;))&#xA;BSkyFormat(as.data.frame(bsky_tree$variable.importance), singleTableOutputHeader=c(&quot;Variable of Importance&quot;))&#xA;&#xA;if({{PlotCVChkbox}})  { plotcp(bsky_tree, main =&quot;Plot of the Complexity Parameter table&quot;)  }                             #plot cp table&#xA;if({{PlotRSQRChkbox}}) { rsq.rpart(bsky_tree) }                       #appropriate for the &quot;anova&quot; method&#xA;bsky_temp = prp(bsky_tree)   &#xA;&#xA;if({{PlotRattle}}) { fancyRpartPlot(bsky_tree) }                         # A better looking plotting from rattle  package&#xA;rm(bsky_tree)&#xA;&#xA;#Useful commands&#xA;#plot(bsky_tree)&#xA;#Summarize the model&#xA;#summary(bsky_tree)&#xA;#Print the cp table&#xA;#printcp(bsky_tree)&#xA;" Helpfile="{x:Null}" splitProcessing="True" MenuLocation="{x:Null}" OutputDefinition="{x:Null}" Width="457" Command="False" Height="420" HelpText="&lt;html&gt;&lt;head&gt;&lt;style type=&quot;text/css&quot;&gt;.c0 { color: Black } .c1 { margin: 0px 0px 10px } .c2 { font-weight: bold; font-size: 14.6666660308838px; line-height: 1.14999997615814; font-family: 'Arial' } .c3 { margin: 0px 0px 13px; line-height: 1.14999997615814; font-size: 11px; font-family: 'Arial' } .c4 { font-weight: normal; font-size: 14.6666660308838px } .c5 { font-weight: bold; font-size: 14.6666660308838px } .c6 { margin: 0px 0px 10px; line-height: 1.14999997615814; font-size: 11px; font-family: 'Arial' } .c7 { font-weight: normal; font-size: 14.6666660308838px; font-family: 'Arial' } .c8 { font-weight: bold; font-size: 14.6666660308838px; font-family: 'Arial' } &lt;/style&gt;&lt;/head&gt;&lt;body class=&quot;c0&quot;&gt;&lt;p class=&quot;c1&quot;&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;Descriptio&lt;/span&gt;&lt;span class=&quot;c2&quot;&gt;n&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c2&quot;&gt;:&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c3&quot;&gt;&lt;span&gt;&lt;span class=&quot;c4&quot;&gt;Fit a rpart model&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;c3&quot;&gt;&lt;span&gt;&lt;span class=&quot;c5&quot;&gt;Usa&lt;/span&gt;&lt;span class=&quot;c5&quot;&gt;ge&lt;/span&gt;&lt;/span&gt;&lt;span&gt;&lt;span class=&quot;c5