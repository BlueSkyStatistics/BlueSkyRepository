<Window x:Class="BSky.Interfaces.Commands.OutputFileLocationDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Output File Location" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" d:DesignHeight="164" d:DesignWidth="420" SizeToContent="WidthAndHeight">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Margin="10,10,10,6" >
            <TextBlock Margin="10" MinWidth="250" Name="txtCommand" ></TextBlock>
            <Button Click="Button_Click"  Margin="10" Width="85">Select File..</Button>
        </StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Grid.Row="1">
            <Button Name="Ok" Margin="5" Width="100" Height="30" Click="Ok_Click">OK</Button>
            <Button Name="Cancel" Margin="5" Width="100" Height="30" Click="Cancel_Click">Cancel</Button>
        </StackPanel>
    </Grid>
</Window>
